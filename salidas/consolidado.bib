    author = "Michael, Rau and Kutza, Jan-Oliver and Werth, Patrick and Liebe, Jan-David and Schöning, Julius",
    booktitle = "2025 IEEE International Symposium on Ethics in Engineering, Science, and Technology (ETHICS)",
    title = "Simulating Vulnerability: An Examination of Ethical, Legal and Social Aspects in the Context of Training Child Protection Workers",
    year = "2025",
    volume = "",
