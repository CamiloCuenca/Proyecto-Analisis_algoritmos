@INPROCEEDINGS{11098253,
    author = "Pavlova, Yoana and Slavov, Vladislav",
    booktitle = "2025 60th International Scientific Conference on Information, Communication and Energy Systems and Technologies (ICEST)",
    title = "Exploring the Ethical Use of AI Technologies/Applications in Academic Environments",
    year = "2025",
    volume = "",
