@INPROCEEDINGS{10895857,
    author = "Preethi, P. and Ragavan, V. and Abinandhana, C. and Umamaheswari, G. and Suvethaa, D.R.",
    booktitle = "2024 International Conference on Emerging Research in Computational Science (ICERCS)",
