@INPROCEEDINGS{10537699,
    author = "Chauhan, Piyush and Sharma, Nishant and Kumar, Alok",
    booktitle = "2023 Seventh International Conference on Image Information Processing (ICIIP)",
    title = "Artificial Intelligence based Vehicular Networks: A step towards Smart Transportation System",
    year = "2023",
    volume = "",
    number = "",
