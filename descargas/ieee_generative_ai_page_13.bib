  ISSN={},
  month={Aug},}@INPROCEEDINGS{11158520,
  author={Davlatova, Madina and Chernobay, Elena},
  booktitle={2025 5th International Conference on Artificial Intelligence and Education (ICAIE)}, 
  title={Collaborative Learning Supported by Generative Artificial Intelligence and Rotation Station Model}, 
  year={2025},
  volume={},
  number={},
  pages={451-454},
  abstract={The paper explores the effectiveness of integrating Generative AI and the ‘station rotation’ model of blended learning to support students' collaborative learning activities. The theoretical basis of the study was the theory of social constructivism. The study employed a triangulation of predominantly qualitative methods, incorporating a qualitative experiment based on the lesson study method, direct observation, and an online survey. A total of 189 students majoring in teacher education participated in the study. The data from the student evaluation of teaching, online survey, students' academic performance, and students' reflection on the classes were analysed to derive the study's results. The findings indicate that the integration of GenAI and the ‘station rotation’ model enhances student engagement in learning, fosters the development of skills in utilising digital tools and AI services, and leads to an improvement in academic performance.},
  keywords={Surveys;Federated learning;Generative AI;Education;Reflection;Hybrid learning;GenAI;digital tools;rotation station;blended learning;collaborative learning},
  doi={10.1109/ICAIE64856.2025.11158520},
  ISSN={},
  month={May},}@INPROCEEDINGS{11041490,
  author={S, Ujjain and D, Dhanush Ishwar and V, Thilak Kumar and D, Srinidhi and E, Vishalini},
  booktitle={2025 3rd International Conference on Artificial Intelligence and Machine Learning Applications Theme: Healthcare and Internet of Things (AIMLA)}, 
  title={Efficient Malware Detection Using Transfer Learning and GAN-Based Data Augmentation}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={Rapid malware evolution calls for the creation of novel, imaginative techniques that enhance the accuracy and resilience of traditional detection methods. Here, a hybrid strategy focused on malware detection will be proposed, where GANs and transfer learning are employed to improve the accuracy and resilience of image representations. Converting malware binaries into grayscale images may change how visual features are used to identify specific patterns that could define malware and eventually determine classification. GANs are used to generate artificial malware samples, which enhance the dataset with a variety of adversarial examples that closely mimic the diversity of actual malware. The model is therefore more resistant to deceptive infection techniques. Convolutional neural network pre-training enables transfer learning and saves resources for lengthy training from scratch and decreases the strain of training. As was previously said, the GAN-generative adversarial examples feature a wider variety of malware types that enhance its capacity to identify new threats precisely. Opposing the traditional approach, the experimental findings showed that this hybrid strategy improved the detection accuracy and decreased the rate of false positives. The reason for this is its adaptability and scalability, which qualify it for real-time malware detection.},
  keywords={Training;Adaptation models;Accuracy;Computational modeling;Transfer learning;Generative adversarial networks;Malware;Real-time systems;Data models;Convolutional neural networks;Malware Detection;Generative Adversarial Networks;Transfer Learning;Image Representation;Convolutional Neural Networks;Cybersecurity},
  doi={10.1109/AIMLA63829.2025.11041490},
  ISSN={},
  month={April},}@INPROCEEDINGS{11016320,
  author={Epaminonda, Epaminondas and Ktorifou, Despo and Michailidis, Maria and Efthymiou, Leonidas},
  booktitle={2025 IEEE Global Engineering Education Conference (EDUCON)}, 
  title={Exploring Students' Familiarity, Usage Patterns, and Perceptions of Generative AI Tools in Education}, 
  year={2025},
  volume={},
  number={},
  pages={1-7},
  abstract={This study explores students' familiarity, usage patterns, and perceptions of Generative AI (GenAI) tools, focusing on their potential applications in education. Using a structured questionnaire, data were collected from 167 respondents to assess familiarity with AI, GenAI, and specific interfaces such as ChatGPT, Dall-E and Google Gemini. Findings indicate that while most respondents are somewhat familiar with AI and GenAI, familiarity with specific tools varies significantly, with ChatGPT being the most recognized and used. Text-based tools dominate usage, primarily for tasks like explaining concepts, summarizing content, and translation. Challenges encountered include issues with accuracy, ease of use, and instruction specificity. Despite these challenges, respondents identify personalized learning resources, automated grading systems, and virtual tutors as the most valuable educational applications. The study highlights the transformative potential of GenAI in education while underscoring the need for improved tool design and expanded training to optimize its adoption.},
  keywords={Training;Productivity;Translation;Accuracy;Generative AI;Focusing;Learning (artificial intelligence);Chatbots;Internet;Engineering education;artificial intelligence;generative AI;tertiary education;student perceptions;AI tools usage patterns;personalized learning},
  doi={10.1109/EDUCON62633.2025.11016320},
  ISSN={2165-9567},
  month={April},}@INPROCEEDINGS{10940731,
  author={Sree Bai, K. Vinaya and Pradeep, K. and Harini, G.V.N.L. and Reddy, N. Yashwanth},
  booktitle={2025 International Conference on Electronics and Renewable Systems (ICEARS)}, 
  title={Mitigating Iterative Fast Gradient Adversarial Poisoning in Archaeological Image Datasets using Generative Adversarial Networks and Denoising Autoencoders}, 
  year={2025},
  volume={},
  number={},
  pages={1586-1591},
  abstract={This paper presents an innovative approach for evaluating and improving the robustness of deep learning models against adversarial attacks for image datasets. In this framework, Generative Adversarial Networks (GANs) are proposed for synthetic image generation, and Denoising Autoencoders (DAEs) are used for noise removal. An adversarial training pipeline is used to generate adversarial examples and analyze the effect of these examples on model performance. A discriminator network is used for adversarial sample detection and classifying samples, whereas the autoencoder is used to minimize reconstruction errors by means of a robust image recovering algorithm. The evaluation contains metrics for an adversarial sample detection and reconstruction accuracy, thereby providing insight into the robustness or sometimes defense of ML models under adversarial conditions. The results indicate that this is able to detect adversarial influences on the models and mitigate them evidently, thus forming one step towards forming secure AI systems.},
  keywords={Deep learning;Training;Renewable energy sources;Autoencoders;Noise reduction;Pipelines;Generative adversarial networks;Robustness;Security;Image reconstruction;Adversarial Attacks;Denoising Autoencoder (DAE);Generative Adversarial Networks (GANs);Adversarial Example Detection;Image Reconstruction;Robust Machine Learning;Poisoning Detection;Deep Learning Security;Adversarial Training;Reconstruction Error},
  doi={10.1109/ICEARS64219.2025.10940731},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10322216,
  author={Yeo, Jin Qi and Wang, Yidi and Tanary, Shawn and Cheng, Jiaxuan and Lau, Matthew and Ng, Aik Beng and Guan, Frank},
  booktitle={2023 IEEE International Symposium on Mixed and Augmented Reality Adjunct (ISMAR-Adjunct)}, 
  title={AICRID: AI-Empowered CR For Interior Design}, 
  year={2023},
  volume={},
  number={},
  pages={837-841},
  abstract={Augmented Reality (AR) technologies have been utilized for interior design for years. Normally 3D furniture models need to be created manually or by scanning with specialized devices and this is usually a costly process. Additionally, users need controllers or hands for manipulating the virtual furniture which may lead to fatigue for long-time usage. Artificial Intelligence (AI) technologies have made it possible to generate 3D models from texts, images or both and show potential to automate interactions through the user’s voice. We propose a novel design, AICRID in short, which aims to automate the 3D model generation and to facilitate the interactions for interior design AR by leveraging on AI technologies. Specifically, our design will allow the users to directly generate 3D furniture models with generative AI, enabling them to directly interact with the virtual objects through their voices.},
  keywords={Solid modeling;Three-dimensional displays;Fatigue;Artificial intelligence;Augmented reality;Augmented Reality;Visualization;Artificial Intelligence;Machine Learning;Interior Design},
  doi={10.1109/ISMAR-Adjunct60411.2023.00184},
  ISSN={2771-1110},
  month={Oct},}@INPROCEEDINGS{11082823,
  author={Helal, Mohamed Medhat and Abu-Elkheir, Mervat and Mashaly, Maggie},
  booktitle={2025 International Conference on Innovation in Artificial Intelligence and Internet of Things (AIIT)}, 
  title={A Generative AI Framework for Cloud Security: Automated Attack Simulation and Threat Detection}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={In the rapidly evolving domain of cloud computing and cybersecurity, staying ahead of threats requires proactive and adaptive solutions. This paper introduces an end-to-end framework designed to enhance cloud security through automated attack generation, execution, and defense enhancement. The framework consists of four main components: the Database, the Generator, the Executor, and the Defender. The database comprises extensive data sources, including attack logs and security reports. The Generator utilizes the environment and system specifications to generate both traditional and zero-day attack vectors. The Executor classifies attacks, assigns them to specialized workers for execution, and consolidates the results into a comprehensive report. The Defender then analyzes this report to iteratively enhance the Intrusion Detection System (IDS). This iterative process enhances the system’s resilience and robustness to future, unknown threats. The framework is adaptable, allowing customization to meet the security needs of various organizations making it well-suited for applications such as enterprise cloud security, cybersecurity education, and beyond. Future work will focus on building a simplified version of the system, which will later be expanded into a scalable architecture capable of addressing more intricate attack scenarios.},
  keywords={Technological innovation;Generative AI;Databases;Cloud computing security;Architecture;Buildings;Computer architecture;Generators;Vectors;Threat assessment;Cloud Security;Generative AI;Cloud Computing;Cyber Security;Retrieval Augmented Generation;Threat Intelligence},
  doi={10.1109/AIIT63112.2025.11082823},
  ISSN={},
  month={May},}@INPROCEEDINGS{10882259,
  author={Dey, Avick Kumar and Chauhan, Vineet Kumar and Singh, Pradeep Kumar and Sarkar, Ananya},
  booktitle={2024 International Conference on Artificial Intelligence and Quantum Computation-Based Sensor Application (ICAIQSA)}, 
  title={Exploring the Integration of Generative AI in Modern Education Systems: A Comprehensive Analysis}, 
  year={2024},
  volume={},
  number={},
  pages={1-8},
  abstract={This paper aims at filling this gap through dissecting the applicability of the Generative AI in the current learning and teaching system. The paper offers an introduction to Generative AI technologies: GPT and DALL-E, and describes the chronological history and current progress of its educational applications. Other objectives include discussing about the uses of Generative AI in adaptive learning environment, smart learning systems, automation of organizational work and in talented minds improvement. Analyzing key successful AI cases, the paper reveals the challenges implementers encountered, and provides debates about the appropriate practices of AI. Research evidence points to the prospect of Generative AI benefiting students and educators in teaching and learning, which indicates the innovative potential of the technology, yet at the same time reveals the potential risks and challenges that need to be addressed concerning the ethical and privacy implications of Generative AI in education. Others may follow general recommendations for policymakers and educators on how to integrate artificial intelligence responsibly, as well as future work for this quickly burgeoning domain.},
  keywords={Ethics;Privacy;Quantum computing;Automation;Generative AI;Law enforcement;Education;Educational technology;Filling;History;Artificial intelligence in education;application of technology in education;adaptive education;Intelligent agents in education;Education technology;Personalized education},
  doi={10.1109/ICAIQSA64000.2024.10882259},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{8843492,
  author={Day, Min-Yuh and Hung, Chi-Sheng},
  booktitle={2019 IEEE 20th International Conference on Information Reuse and Integration for Data Science (IRI)}, 
  title={AI Affective Conversational Robot with Hybrid Generative-Based and Retrieval-Based Dialogue Models}, 
  year={2019},
  volume={},
  number={},
  pages={403-409},
  abstract={ChatBot technology has become a widely used in various application fields. An important topic in the research on conversational robots is the improvement of their temperature during operation for enhanced user interaction. In this study, we propose an artificial intelligence affective conversational robot (AIACR), which is an integration of an artificial intelligence deep learning sentiment analysis model and generative-and retrieval-based dialogue models. The sentiment analysis model developed in this study uses three models, namely, multilayer perceptron (MLP), long short-term memory (LSTM) and bidirectional long short-term memory (BiLSTM). Moreover, word2vec and semantics are utilized as the basis for similarity ranking models. The deep learning dialogue model, sentiment analysis model, and similarity model were integrated and compared as well. The experimental results show that the sentiment analysis model, similarity model, and dialogue model respectively utilize BiLSTM, word2vec, and the retrieval-based model to achieve the best dialogue performance. The major research contributions of this study are the developed AIACR and the proposed affective conversational robot index (ACR Index) as a criterion for evaluating the effectiveness of emotional dialogue robots.},
  keywords={Robots;Analytical models;Sentiment analysis;Predictive models;Systems architecture;Training;Artificial Intelligence, ChatBot, Deep Learning, Natural Language Processing, Sentiment Analysis},
  doi={10.1109/IRI.2019.00068},
  ISSN={},
  month={July},}@ARTICLE{10712168,
  author={Kolomeets, Maxim and Wu, Han and Shi, Lei and van Moorsel, Aad},
  journal={IEEE Transactions on Computational Social Systems}, 
  title={The Face of Deception: The Impact of AI-Generated Photos on Malicious Social Bots}, 
  year={2025},
  volume={12},
  number={3},
  pages={1080-1091},
  abstract={In this research, we investigate the influence of utilizing artificial intelligence (AI)-generated photographs on malicious bots that engage in disinformation, fraud, reputation manipulation, and other types of malicious activity on social networks. Our research aims to compare the performance metrics of social bots that employ AI photos with those that use other types of photographs. To accomplish this, we analyzed a dataset with 13 748 measurements of 11 423 bots from the VK social network and identified 73 cases where bots employed generative adversarial network (GAN)-photos and 84 cases where bots employed diffusion or transformers photos. We conducted a qualitative comparison of these bots using metrics such as price, survival rate, quality, speed, and human trust. Our study findings indicate that bots that use AI-photos exhibit less danger and lower levels of sophistication compared to other types: AI-enhanced bots are less expensive, less popular on exchange platforms, of inferior quality, less likely to be operated by humans, and, as a consequence, faster and more susceptible to being blocked by social networks. We also did not observe any significant difference between GAN-based and diffusion/transformers-based bots, indicating that diffusion/transformers models did not contribute to increased bot sophistication compared to GAN models. Our contributions include a proposed methodology for evaluating the impact of photos on bot sophistication, along with a publicly available dataset for other researchers to study and analyze bots. Our research findings suggest a contradiction to theoretical expectations: in practice, bots using AI-generated photos pose less danger.},
  keywords={Artificial intelligence;Generative adversarial networks;Social networking (online);Transformers;Face recognition;Neural networks;Faces;Image synthesis;Diffusion models;Databases;Artificial intelligence (AI)-generated photographs;bot evolution;diffusion;disinformation;generative adversarial network (GAN);social bots;social networks},
  doi={10.1109/TCSS.2024.3461328},
  ISSN={2329-924X},
  month={June},}@INPROCEEDINGS{11046215,
  author={Wu, Miao-Fei and Chen, Rain},
  booktitle={2025 13th International Conference on Information and Education Technology (ICIET)}, 
  title={A Preliminary Exploration of the Cooperation Between Humans and AI in the Creation of Picture Books}, 
  year={2025},
  volume={},
  number={},
  pages={462-465},
  abstract={Generative Artificial Intelligence (GAI) has shown great potential to assist in the creation of picture books. The fairy tale of The Little Match Girl was adapted in this study to explore the application of GAI in “story creation” and “illustration creation” by combining human creativity and AI generation technology to produce a six-page picture book. Furthermore, the performance of AI and a human in the process of the adaption was compared and analyzed. During story creating stage, the researcher of this study employed ChatGPT to generate story structure and creative elements. ChatGPT had a great accomplishment of condensing and narrating stories, but its adjustment on emotional tones required human interventions to achieve the desired effect. Although there were several modifications prior to the ultimate ending to “war” and “tragedy” requested by the researcher, ChatGPT did still show excellent creative potential. When creating illustrations, the researcher used ChatGPT to generate commands and Midjourney to generate illustrations. With character setting and costume details requiring human tweaking and integration, Midjourney excelled at color grading and atmosphere simulation, but having consistency between details and style was a challenging task for Midjourney. In the process of selecting and modifying generated content, AI tools played an important role in inciting creative ideas. The results of this study indicated that GAI was feasible to improve creation efficiency and inspire creativity; however, human intervention was still needed in details and accuracy. As an auxiliary tool, AI could effectively support artistic creation, but achieving the desired results required fine adjustment by humans. It is hoped that this study can serve as a reference direction for future artistic creation.},
  keywords={Accuracy;Generative AI;Atmospheric modeling;Education;Color;Chatbots;Artificial intelligence;Creativity;AI;ChatGPT;Midjourney;Picture Book},
  doi={10.1109/ICIET66371.2025.11046215},
  ISSN={},
  month={April},}@INPROCEEDINGS{11147291,
  author={Marimekala, Daniel},
  booktitle={2025 IEEE Integrated STEM Education Conference (ISEC)}, 
  title={Impact of GenAI using Auto Content Generation Tools on Cognitive Thinking Process in Academia}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={In today’s world, there is a drastic change in how tools are auto-generating content. These content generation tools that use generative AI make it easier to write blogs, generate content, reply to emails, create and publish content quickly, parse text, and refer to specific information very quickly and with ease. Students use these tools in colleges and educational institutions daily on social media. GenAI tools help reconstruct sentences, correct grammar, use proper words, auto-filling information, and preempt words that make the user not think but instead follow the prompts. GenAI will impact future generations because they cannot be self-reliant and rely heavily on artificial intelligence to form sentences and write the essential content. There must be a need for AI awareness and governance adoption in AI applications and tools as an integral process with a balance on how AI adoption in educational and social networks should enrich and promote individuals in the learning process and dynamically adjust to the individual requirements and help them in crafting sentences intelligently, build information and ideas that encourage thinking process, and using standard English conventions and expression of ideas. The primary focus of this paper is to introduce the challenges of GenAI adoption in applications and tools we use daily and its exposure to Gen X, Gen Y, Gen Z, and Gen Alpha. In this paper, we will bring awareness to such GenAI tools that can adversely impact the growth of individual cognitive thinking processes and the measures that citizens should take so that Human Intelligence prevails over Artificial Intelligence.},
  keywords={Learning systems;Productivity;Ethics;Technological innovation;Social networking (online);Rail to rail inputs;Generative AI;Market research;Artificial intelligence;Usability;AI Applications;AI Tools;ChatGPT;Cognitive Thinking;Content Generation;Guardrails;Gemini;Governance;Large Language Models (LLM)},
  doi={10.1109/ISEC64801.2025.11147291},
  ISSN={2473-7623},
  month={March},}@INPROCEEDINGS{10871269,
  author={Morano-Okuno, Hector Rafael and Esqueda-Merino, Donovan M. and Chong-Quero, J. Enrique and Sandoval-Benitez, Guillermo and Murano-Labastida, Daishi Alfredo and Jaramillo-Godinez, Ricardo},
  booktitle={2024 International Conference on Intelligent Computing and Next Generation Networks (ICNGN)}, 
  title={Exploring the Capacity of a Generative Design Software Application Based on AI Applied to the Design of Sports Foot Prostheses}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={Thanks to the development of Artificial Intelligence in Computational Materials Science., new software applications can predict the behavior of materials when subjected to different conditions. In Product Design., these computational tools are of great help because they offer users different alternatives when selecting materials to ensure the durability of their designs. Combining the advances of Artificial Intelligence., Computational Materials Science., and Product Design., various software applications emerge that contain these algorithms programmed to use Generative Design. This article explores the scopes of a software tool application based on Generative Design., which is applied to developing lower limb prostheses aimed at being used by children since the geometries obtained can be attractive to them and reduce the adverse effects of their perception when using them. A case study describes the methodology employed to design this type of prosthesis. The results found that the software application used for the Generative Design is remarkably user-friendly., providing a sense of reassurance and confidence in its usability. Another result was that the boundary conditions were identified to design different kinds of prostheses., in addition to the results offered by the application software.},
  keywords={Limbs;Software algorithms;Product design;Artificial intelligence;Usability;Software tools;Next generation networking;Computational materials science;Sports;Prosthetics;artificial intelligence;computational materials science;generative design;lower limb prosthesis;educational innovation;higher education;professional education},
