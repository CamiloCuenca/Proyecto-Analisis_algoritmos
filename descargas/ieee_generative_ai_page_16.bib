  booktitle={2025 6th International Conference on Intelligent Communication Technologies and Virtual Mobile Networks (ICICV)}, 
  title={Medical Plant Identification and Utilities Recommendation using Ensemble Techniques}, 
  year={2025},
  volume={},
  number={},
  pages={623-627},
  abstract={This paper presents a deep learning-based system capable of classifying medicinal and Ayurvedic plants with high accuracy, countering the increasing decoupling with traditional knowledge as a result of increasing urbanization. Unlike traditional identification methods, which are generally manual and expert-dependent, the solution presented here provides high-accuracy classification as well as informative utility. The plant is not just identified, but other details like its scientific name, medicinal applications, and suggested home remedies are also provided. The system is implemented using a hybrid ensemble of InceptionV3 and ConvNeXtTiny models, trained on a heterogeneous data set of 30 medicinal plants in order to provide secure and reliable classification.To recommend appropriate treatments, a collaborative filtering approach is used to compare the identified plant to Ayurvedic treatments in a structured CSV database. The system is accessed via a Streamlit-based web portal and an Android smartphone app, enabling real-time, on-the-spot plant identification and remedy recommendation. Model performance is measured in terms of accuracy, which decides its deployment readiness. With the integration of traditional Ayurvedic practices and modern artificial intelligence, this system provides a pragmatic, user-friendly tool for promoting the use of natural medicine and public awareness of plant-based health remedies.},
  keywords={Medicinal plants;Deep learning;Accuracy;Databases;Feature extraction;Real-time systems;Data models;Reliability;Ensemble learning;Biomedical imaging;Medicinal Plants;Ayurvedic Remedies;Ensemble Methods;Deep Learning;Personalized Recommendations},
  doi={10.1109/ICICV64824.2025.11085644},
  ISSN={},
  month={June},}@INPROCEEDINGS{9977500,
  author={Li, Jianxue and Xiao, Yang and Wu, Jiawei and Chen, Yaozhi and Liu, Jun},
  booktitle={2022 IEEE 33rd Annual International Symposium on Personal, Indoor and Mobile Radio Communications (PIMRC)}, 
  title={Attentive Dual-Head Spatial-Temporal Generative Adversarial Networks for Crowd Flow Generation}, 
  year={2022},
  volume={},
  number={},
  pages={800-806},
  abstract={Crowd flow generation, which aims to simulate the flows of crowd in the future is of great importance to many real-life applications including epidemic spreading and traffic management. The challenges of accurate crowd flow generation come from both the complex spatial-temporal correlations of crowd flow data and the small amount of training data, which is largely not well studied and addressed in existing works. In this paper, we propose a novel attentive dual-head spatial-temporal generative adversarial network entitled ADST-GAN to simulate multi-step crowd flow. To augment the learning power of the generator, we adopt attentive temporal queue and self attention mechanism to automatically capture the complex global spatial-temporal dependencies of crowd flow data. As for the discriminator, we design a dual-head architecture with two-objective training to avoid the negative effects of quick overfitting. To evaluate the effectiveness of the proposed method, we conduct extensive experiments over the bike and taxi trip datasets in New York. The results demonstrate the proposed method outperforms seven state-of-the-art baselines significantly in terms of the quality of simulated crowd flow data.},
  keywords={Training;Epidemics;Correlation;Training data;Predictive models;Generative adversarial networks;Feature extraction;Crowd flow generation;generative adversarial networks;attention mechanism},
  doi={10.1109/PIMRC54779.2022.9977500},
