  title={Artificial Intelligence for EM Imaging: Recent Advances and Future Trends}, 
  year={2023},
  volume={},
  number={},
  pages={48-49},
  abstract={Artificial Intelligence (AI) has been recently attracting a lot of research efforts to develop accurate and computationally-fast methodologies for electromagnetic (EM) imaging. This work overviews some of the most recent advances in this emerging field, by also envisaging some future trends and research directions according to the authors' vision.},
  keywords={Antenna measurements;Deep learning;Imaging;Market research;Artificial intelligence;Electromagnetics;Electromagnetic (EM) Imaging;Inverse Problems (IPs);Three-Steps Learning-by-Examples (3-LBE) System-by-Design (SbD);Physics-Embedded Deep Learning (PE-DL)},
  doi={10.1109/CAMA57522.2023.10352857},
  ISSN={2643-6795},
  month={Nov},}@INPROCEEDINGS{10920248,
  author={Wu, Ye and Wei, Qinggong and Song, Ruiqi and Cui, Chenglin and Li, Xinqing and Zhu, Fenghua and Ai, Yunfeng},
  booktitle={2024 IEEE 27th International Conference on Intelligent Transportation Systems (ITSC)}, 
  title={A Generative Active Learning Framework for Semantic Segmentation in Autonomous Driving}, 
  year={2024},
  volume={},
  number={},
  pages={2979-2984},
  abstract={Active learning is a machine learning or artificial intelligence method that aims to achieve optimal model performance by actively selecting and labeling the most valuable samples, using the fewest high-quality labeled samples. Relevant works have already been conducted using information uncertainty to select valuable samples. However, its effectiveness for semantic segmentation tasks is relatively poor. In this paper, we propose an active learning framework based on the Generative Adversarial Networks (GANs) architecture for semantic segmentation task, which achieves efficient data selection while simultaneously enhancing semantic segmentation performance. The proposed framework combines the generator (i.e., semantic segmentation network) and the discriminator (i.e., data selection network) for jointly optimizing of both semantic segmentation and data selection tasks. Extensive experiments were conducted on the dataset, which was collected from real-world autonomous driving scenes. Experimental results demonstrate that our method achieved competitive results compared to other SOTA methods.},
  keywords={Uncertainty;Semantic segmentation;Active learning;Generative adversarial networks;Diffusion models;Generators;Labeling;Iterative methods;Autonomous vehicles;Intelligent transportation systems;Active learning;autonomous driving;semantic segmentation;generative adversarial networks},
  doi={10.1109/ITSC58415.2024.10920248},
  ISSN={2153-0017},
  month={Sep.},}@INPROCEEDINGS{10687906,
