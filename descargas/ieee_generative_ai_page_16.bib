  author={Yangoti, Sai Bhavya and Dumpala, Rohith and Punyamurthy, Vijay and Jatoth, Ravi Kumar},
  booktitle={2024 International Conference on Artificial Intelligence and Emerging Technology (Global AI Summit)}, 
  title={Illumination Effects on Deep Fake Detection: Performance Comparison of CNN Architectures}, 
  year={2024},
  volume={},
  number={},
  pages={184-189},
  abstract={While AI has many advantages, it also has a few drawbacks associated with “deep fakes.” Deep fakes involve the creation of very realistic videos and images realized through technologies that work with facial and voice modalities for deceptive purposes. Researchers have developed various techniques to detect these manipulated videos and images, employing complex neural network models such as convolutional neural networks (CNNs). However, deep fakes are very difficult to identify due to lighting and shadow effects that may hide some signs of manipulation. No one technique of detection could have the ability of identifying the tampered images under all kinds of illumination conditions. This paper compares different CNN models-ResNet50, EfficientNet, and MobileNetV2-to improve detection accuracy so as to be more resistant to new deep fake techniques, in particular under different illumination conditions. The importance of the study lies in arriving at which of the considered CNN models turns out to be most accurate under a specific illumination effect. Such an understanding could, therefore, improve the results of detection even when applied to cases of tampered faces in fake Aadhaar detection and fake id detection, thereby clearly bringing out the usefulness of the research in practice.},
  keywords={Resistance;Deepfakes;Accuracy;Lighting;Computer architecture;Software;Convolutional neural networks;Reliability;Artificial intelligence;Protection;deep fakes;tampered images;illumination;CNN;fake Aadhar detection},
  doi={10.1109/GlobalAISummit62156.2024.10947810},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10808120,
  author={Wu, Qiong and Li, Jiajia},
  booktitle={2024 5th International Conference on Intelligent Computing and Human-Computer Interaction (ICHCI)}, 
  title={Journalists' Technological Trust and Willingness to Use Generative AI: A Perspective Based on Risk Perception Theory}, 
  year={2024},
  volume={},
  number={},
  pages={526-530},
  abstract={This study explores the impact of perceived risk and trust on the willingness of journalists to use generative AI (GenAl) technology. As AI evolves, its applications in journalism, such as automated news writing and personalized content recommendations, have grown significantly. However, the adoption of GenAl by journalists is influenced by various factors, with perceived risk and trust being paramount. Perceived risk encompasses the uncertainties and potential negative outcomes associated with using the technology, while trust pertains to the confidence in the technology's reliability, security, and effectiveness. This research aims to fill the gap in empirical studies on these factors by constructing a theoretical model based on risk perception and trust theories. The study employs a detailed survey, validated through confirmatory factor analysis (CFA), to measure journalists' perceived risks, trust, and their willingness to use GenAl. Findings from this research are expected to offer valuable insights for the journalism industry, providing strategies to mitigate perceived risks and enhance trust, thereby promoting broader adoption of GenAl. Additionally, the results will contribute to the theoretical understanding of technology adoption in journalism and other professional domains.},
  keywords={Industries;Surveys;Privacy;Uncertainty;Generative AI;Psychology;Writing;Reliability theory;Journalism;Security;Generative AI;perceived risk;trust;willingness to use;Journalism;technology adoption},
