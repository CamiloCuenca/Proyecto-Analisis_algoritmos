  ISSN={},
  month={April},}@INPROCEEDINGS{10588790,
  author={Andersen, Mathias Viborg and Greer, Ross and Møgelmose, Andreas and Trivedi, Mohan M.},
  booktitle={2024 IEEE Intelligent Vehicles Symposium (IV)}, 
  title={Learning to Find Missing Video Frames with Synthetic Data Augmentation: A General Framework and Application in Generating Thermal Images Using RGB Cameras}, 
  year={2024},
  volume={},
  number={},
  pages={104-109},
  abstract={Advanced Driver Assistance Systems (ADAS) in intelligent vehicles rely on accurate driver perception within the vehicle cabin, often leveraging a combination of sensing modalities. However, these modalities operate at varying rates, posing challenges for real-time, comprehensive driver state monitoring. This paper addresses the issue of missing data due to sensor frame rate mismatches, introducing a generative model approach to create synthetic yet realistic thermal imagery. We propose using conditional generative adversarial networks (cGANs), specifically comparing the pix2pix and CycleGAN architectures. Experimental results demonstrate that pix2pix outperforms CycleGAN, and utilizing multi-view input styles, especially stacked views, enhances the accuracy of thermal image generation. Moreover, the study evaluates the model’s generalizability across different subjects, revealing the importance of individualized training for optimal performance. The findings suggest the potential of generative models in addressing missing frames, advancing driver state monitoring for intelligent vehicles, and underscoring the need for continued research in model generalization and customization.},
  keywords={Training;Accuracy;Intelligent vehicles;Image synthesis;Learning (artificial intelligence);Thermal sensors;Generative adversarial networks;image synthesis;generative artificial intelligence;thermal imagery;pseudo-labeled dataset;data augmentation},
  doi={10.1109/IV55156.2024.10588790},
  ISSN={2642-7214},
  month={June},}@INPROCEEDINGS{10531477,
  author={Singh, Ritesh and Singh, Swati and Singh, Amulya},
  booktitle={2024 2nd International Conference on Artificial Intelligence and Machine Learning Applications Theme: Healthcare and Internet of Things (AIMLA)}, 
  title={Landmark events, major trends, and the future of AI research: history, promise, hype}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={In the present paper, we look at the trends in Artificial Intelligence research, as seen from academic publications indexed by the Google Scholar search engine. More specifically, we look at the number of publications including AI related keywords for every calendar year from 1960 to 2023. By plotting the number of publications and the annual growth rate for every year, we are able to identify major trends and landmark events in the history of AI research and development. We discuss them in detail in this paper, along with extrapolation into the future. We also highlight the repeated historical tendency of human society to overestimate the imminent capabilities of AI systems in the ‘very near future’.},
  keywords={Generative AI;Pandemics;Medical services;Machine learning;Search engines;Market research;History;Artificial Intelligence;AI;trends;history;hype},
  doi={10.1109/AIMLA59606.2024.10531477},
  ISSN={},
  month={March},}@INPROCEEDINGS{11008162,
