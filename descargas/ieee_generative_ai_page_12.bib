  booktitle={2025 5th International Conference on Innovative Research in Applied Science, Engineering and Technology (IRASET)}, 
  title={AI-Driven Generative Modeling and Optimization for Ethical and Scalable Selection Systems: A Multi-Domain Scoping Review}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={Generative Artificial Intelligence (GAI) transforms selection and recommendation systems by enabling automated optimization, enhanced personalization, and intelligent decision-making. This scoping review surveys the integration of GAI-driven selection models across domains, such as recruitment, healthcare, education, and e-commerce, demonstrating improvements in accuracy, efficiency, and scalability. AI-enhanced personal selection, healthcare decision support systems, and generative adversarial networks (GANs) for personalized selection illustrate the impact of synthetic data generation and deep learning on optimizing user experience. Despite these advancements, ethical concerns, including algorithmic bias, transparency, and data privacy, remain important challenges. By consolidating state-of-the-art approaches and methodologies, this review maps key advancements, identifies research gaps, and proposes future directions to ensure responsible and scalable deployment of Generative AI in selection and recommendation systems.},
  keywords={Ethics;Reviews;Generative AI;Education;Medical services;Generative adversarial networks;Electronic commerce;Recommender systems;Optimization;Recruitment;Generative Artificial Intelligence;GAI;Recommendation Systems;Automated Optimization;Selection Systems;generative adversarial networks;GANs;Recruitment;Healthcare;Education;Ethical AI;Algorithmic bias},
  doi={10.1109/IRASET64571.2025.11008162},
  ISSN={},
  month={May},}@INPROCEEDINGS{11158886,
  author={Nambiar, Rajashree and Nanjundegowda, Raghu},
  booktitle={2025 International Conference on Computing Technologies & Data Communication (ICCTDC)}, 
  title={Tooth Image Synthesis Using Customized GAN Architectures: A Deep Learning-Based Solution for Data Scarcity in Dentistry}, 
  year={2025},
  volume={},
  number={},
  pages={1-7},
  abstract={The scarcity of high-quality, annotated dental radiographic data poses a significant challenge to the advancement of Artificial Intelligence (AI) applications in dentistry. This study proposes a customized Generative Adversarial Network (GAN) framework for the generation of anatomically realistic synthetic dental images, focusing on individual tooth-level synthesis from panoramic radiographs. Utilizing a dataset of 612 annotated images categorized according to the FDI tooth notation system, the model was trained separately on cropped single-tooth images and full panoramic views. Architectural innovations such as residual blocks, self-attention modules, and an auxiliary classifier were incorporated to enhance image fidelity and stability. Quantitative evaluation using Fréchet Inception Distance (FID) demonstrated superior image quality in single-tooth generation compared to panoramic synthesis, with the best-performing classes achieving FID scores below 100. These results validate the model's potential to augment dental datasets for downstream AI applications, while promoting ethical data practices by reducing the dependency on clinical image acquisition.},
  keywords={Radiography;Image quality;Training;Technological innovation;Image synthesis;Teeth;Generative adversarial networks;Stability analysis;Dentistry;Artificial intelligence;Artificial Intelligence;Convolutional Neural Networks;Dentistry;Generative Adversarial Networks;Synthetic image},
