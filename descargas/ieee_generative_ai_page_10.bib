  title={Work in Progress: AI and Fashion Trends - An Exploration To The Topic}, 
  year={2025},
  volume={},
  number={},
  pages={1-4},
  abstract={This research-in-progress paper delves into the rationale behind luxury brands using AI to improve trend forecasting and scenario modeling in the fashion industry. It includes a review of 23 articles published between 2012 and 2024 on IEEE Xplore, as well as an analysis of public discourse initiated by luxury brands on AI's role in fashion. As part of a larger project, this paper aims, primarily, to present the key points of emerging debate.},
  keywords={Reviews;Predictive models;Market research;Data processing;Clothing industry;Software;Artificial intelligence;Forecasting;Engineering education;Machine intelligence;AI;Design;Fashion;trends;forecasting},
  doi={10.1109/EDUNINE62377.2025.10981407},
  ISSN={},
  month={March},}@INPROCEEDINGS{10947971,
  author={Verma, Goldy},
  booktitle={2024 International Conference on Artificial Intelligence and Emerging Technology (Global AI Summit)}, 
  title={Eye Disease Classification Enhanced by EfficientNetB3: A Deep Learning Approach}, 
  year={2024},
  volume={},
  number={},
  pages={1016-1021},
  abstract={Deep learning and computer vision have especially improved medical diagnostics in ophthalmology and other fields. Eye diseases include cataracts, diabetic retinopathy, and glaucoma, and normal problems offer major challenges considering their frequency and impact on vision health. Conventional diagnosis methods rely on the subj ective and prone-to-error manual examination that ophthalmologists provide. This work investigates the use of a state-of-the-art deep learning architecture, Fine Tuned EfficientN etB3, for the automated categorization of various eye illnesses. We use a large collection of 3,424 high-quality retinal pictures taken from several repositories and annotated by professionals. We raise the quality of the dataset by using rigorous preprocessing and augmentation techniques. Fine-tuned via transfer learning, the EfficientN etB3 model shows great performance with a 94% general accuracy. Evaluations of precision, recall and F l-score show the consistency of the model across several disease diagnoses. Particularly in underdeveloped areas, the model's exceptional accuracy and efficiency emphasize its capacity to improve clinical operations, raise diagnosis accuracy, and give access to professional eye care. Our efforts provide scalable solutions for early and accurate eye disease diagnosis, therefore advancing the expanding field of artificial intelligence-driven medical diagnostics.},
  keywords={Deep learning;Cataracts;Training;Glaucoma;Diabetic retinopathy;Accuracy;Computational modeling;Transfer learning;Medical diagnosis;Artificial intelligence;Artificial Intelligence;Deep Learning;Eye Disease Classification;Model Training;Fine Tuned EfficientN etB3 Model},
  doi={10.1109/GlobalAISummit62156.2024.10947971},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10896224,
  author={Braccini, Michele and De Filippo, Allegra and Lombardi, Michele and Milano, Michela},
  booktitle={2025 IEEE International Conference on Artificial Intelligence and eXtended and Virtual Reality (AIxVR)}, 
  title={Dance Choreography Driven by Swarm Intelligence in Extended Reality Scenarios: Perspectives and Implications}, 
  year={2025},
  volume={},
  number={},
  pages={348-354},
  abstract={Recent developments in artificial intelligence (AI) foster creativity in all its forms. In particular, with the advent of extended reality (XR), we have witnessed an explosion of once unimaginable artistic creations and products, one above all the Metaverse. Dance and the process of creating dance choreography can benefit from the use of these new technologies, but so far little has been done in this regard. This paper provides an overview of the existing literature on the use of AI tools in combination with XR technology within the artistic context of dance. Furthermore, we propose new application scenarios that involve human interactions mediated by XR technologies. In particular, we focus on dance choreography performed by artificial agents driven by Swarm Intelligence mechanisms, and we discuss the perspectives and potential implications for both artificial and human creativity through the integration with XR.},
  keywords={Performance evaluation;Humanities;Visualization;Translation;Extended reality;Generative AI;Metaverse;Proposals;Particle swarm optimization;Creativity;Swarm Intelligence;Dance Choreography;Dancing in Extended Reality},
  doi={10.1109/AIxVR63409.2025.00066},
