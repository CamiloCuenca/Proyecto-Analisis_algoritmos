  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/10951256},}@INPROCEEDINGS{10423429,
  author={Yadav, Priya and Kaur, Ramandeep and Hariharan, U.},
  booktitle={2023 7th International Conference on Electronics, Materials Engineering & Nano-Technology (IEMENTech)}, 
  title={A Study on Customized Face generator using Generative Adversarial Networks (GANs)}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={The Customized Face Generator (CFG) emerges as a ground-breaking response to a substantial challenge in the rapidly developing fields of computer graphics and artificial intelligence. Background issues for this study include the need to address ethical issues related to bias and representation in these digital identities as well as the growing desire for realistic and adaptable human faces across a range of industries, from entertainment to digital marketing. The design and development of a system that effortlessly blends diversity, realism, and personalization while preserving ethical standards is the main issue this research attempts to solve. It intends to provide users with the tools to create distinctive faces while upholding individual identities and avoiding damaging prejudices in generated content. The CFG’s ability to transform digital representation is its primary significance. It allows users to generate a variety of lifelike faces by utilizing cutting-edge deep learning techniques like GANs and VAEs, making for more engaging digital experiences and fostering greater individual expression. Modern technologies and ethical considerations are incorporated into the study approach. The architecture of the CFG is made to gradually add layers of complexity to faces as they are generated, giving them a more realistic appearance. In order to prevent offensive content, an easy-to-use customizing interface is created. The key result of from this research project includes a face creation system that is extremely adjustable and morally upright. The usefulness of the CFG is shown through quantitative metrics assessing diversity, realism, and user happiness together with qualitative evaluations. The CFG has enormous global relevance. It opens up a new level of inclusion and personalization in digital settings, having an impact on fields that depend on visual communication. This study adds to the ongoing conversation around moral AI and lays out a plan for responsible innovation, where technology meets both practical needs and social conscience.},
  keywords={Training;Ethics;Generative adversarial networks;Generators;Artificial intelligence;Faces;Convergence;discriminator;deep learning;generator;Generative adversarial Network (GAN);personalization;Variational Auto encoder (VAE)},
  doi={10.1109/IEMENTech60402.2023.10423429},
  ISSN={2767-9934},
  month={Dec},}@INPROCEEDINGS{10282462,
  author={Shaik, Riyaaz Uddien and Alipour, Mohamad and Taciroglu, Ertugrul},
  booktitle={IGARSS 2023 - 2023 IEEE International Geoscience and Remote Sensing Symposium}, 
  title={A Bibliometric Analysis of Artificial Intelligence-Based Solutions to Challenges in Wildfire Fuel Mapping}, 
  year={2023},
  volume={},
  number={},
  pages={1610-1613},
  abstract={Wildfire fuel mapping plays a vital role in understanding and mitigating the risks associated with wildfires. This study conducts a comprehensive analysis of existing literature to investigate the prevailing trends in wildfire fuel mapping, including an analysis of the satellite sensors commonly used for fuel mapping, the predominant types of fuels mapped, the resolution of fuel maps to assess accuracy and detail, and the publication trends over the years to understand the growth and interest in the field. By leveraging AI techniques including machine learning, we review solutions to overcome challenges such as data scarcity, modalities, mapping understory vegetation, model explainability, and lack of uncertainty-aware models. This manuscript aims to serve as a reference for researchers and practitioners seeking to advance the field of wildfire fuel mapping through data-driven and AI-powered approaches.},
  keywords={Satellites;Bibliometrics;Vegetation mapping;Machine learning;Market research;Data models;Sensors;artificial intelligence;fuel mapping;remote sensing;review;wildfires},
  doi={10.1109/IGARSS52108.2023.10282462},
  ISSN={2153-7003},
  month={July},}@INPROCEEDINGS{10579362,
  author={Stankovic, Marko and Bacanin, Nebojsa and Zivkovic, Miodrag and Jovanovic, Luka and Sarac, Marko and Antonijevic, Milos},
  booktitle={2024 Zooming Innovation in Consumer Technologies Conference (ZINC)}, 
  title={Generative Adversarial Network for Data Augmentation and Substitution}, 
  year={2024},
  volume={},
  number={},
  pages={7-12},
  abstract={The healthcare industry undergoes a transformative shift with the integration of artificial intelligence (AI), particularly in medical imaging analysis. AI algorithms, trained on extensive datasets, demonstrate accuracy comparable to medical experts, promising earlier diagnoses and improved outcomes. However, challenges in widespread AI adoption include limited access to high-quality medical datasets and concerns about data privacy. These challenges hinder progress, as existing datasets often suffer from obsolescence. Overcoming these limitations is crucial for advancing AI research in healthcare. Leveraging Generative Adversarial Networks (GANs) addresses data scarcity by generating synthetic medical data that maintains essential features without compromising patient privacy. GANs present a solution to challenges related to data availability and quality, unlocking new opportunities for innovation in medical AI research and clinical practice.},
  keywords={Data privacy;Technological innovation;Accuracy;Neural networks;Medical services;Generative adversarial networks;Data models;Medical Data;Generative Adversarial Network;Data Substitution;Data Synthesis;Convolutional neural networks},
  doi={10.1109/ZINC61849.2024.10579362},
  ISSN={},
  month={May},}@INPROCEEDINGS{11016607,
  author={Jimenez, Patricia P. and Mejía, Andrés and Pascual, Jimena},
  booktitle={2025 IEEE Global Engineering Education Conference (EDUCON)}, 
  title={Leveraging AI to Foster Critical Thinking in Engineering Education: Exploring the Three Spheres of Critical Thinking}, 
  year={2025},
  volume={},
  number={},
  pages={1-3},
  abstract={Critical thinking is widely recognized as essential for education and professional practice, particularly as artificial intelligence (AI) transforms engineering education and work. This paper examines the relationship between AI and critical thinking in engineering through Mejía's three-sphere framework: intelligent tool use, comparative analysis of methods, and socio-technical impacts. We propose a research methodology to evaluate how generative AI can enhance critical thinking capabilities among engineering students. The study employs a mixed-methods approach across two engineering courses, with a framework that focuses on engineering-specific cognitive processes while integrating technical and sociotechnical considerations throughout the engineering lifecycle.},
  keywords={Generative AI;Cognitive processes;Transforms;Engineering students;artificial intelligence;critical thinking;engineering education},
  doi={10.1109/EDUCON62633.2025.11016607},
  ISSN={2165-9567},
  month={April},}@ARTICLE{9233366,
  author={Tjoa, Erico and Guan, Cuntai},
  journal={IEEE Transactions on Neural Networks and Learning Systems}, 
  title={A Survey on Explainable Artificial Intelligence (XAI): Toward Medical XAI}, 
  year={2021},
  volume={32},
  number={11},
  pages={4793-4813},
  abstract={Recently, artificial intelligence and machine learning in general have demonstrated remarkable performances in many tasks, from image processing to natural language processing, especially with the advent of deep learning (DL). Along with research progress, they have encroached upon many different fields and disciplines. Some of them require high level of accountability and thus transparency, for example, the medical sector. Explanations for machine decisions and predictions are thus needed to justify their reliability. This requires greater interpretability, which often means we need to understand the mechanism underlying the algorithms. Unfortunately, the blackbox nature of the DL is still unresolved, and many machine decisions are still poorly understood. We provide a review on interpretabilities suggested by different research works and categorize them. The different categories show different dimensions in interpretability research, from approaches that provide “obviously” interpretable information to the studies of complex patterns. By applying the same categorization to interpretability in medical research, it is hoped that: 1) clinicians and practitioners can subsequently approach these methods with caution; 2) insight into interpretability will be born with more considerations for medical practices; and 3) initiatives to push forward data-based, mathematically grounded, and technically grounded medical education are encouraged.},
  keywords={Artificial intelligence;Machine learning;Medical information systems;Machine learning algorithms;Explainable artificial intelligence (XAI);interpretability;machine learning (ML);medical information system;survey},
  doi={10.1109/TNNLS.2020.3027314},
  ISSN={2162-2388},
  month={Nov},}@INPROCEEDINGS{9141685,
  author={Zhang, Zhongwei},
  booktitle={2020 IEEE 5th Information Technology and Mechatronics Engineering Conference (ITOEC)}, 
  title={Research Progress on Generative Adversarial Network with its Applications}, 
  year={2020},
  volume={},
  number={},
  pages={396-399},
  abstract={As a new unsupervised learning algorithm framework, generative adversarial networks(GAN) have been favored by more and more researchers, and it has become a research hotspot now. GAN is inspired by the two-person zero-sum game theory in game theory. Its unique adversarial training idea can generate high-quality samples and has more powerful feature learning and feature expression capabilities than traditional machine learning algorithms. At present, GAN has achieved remarkable success in the field of computer vision, especially in the field of sample generation. Every year, a large number of GAN-related research papers are produced, reflecting the fiery degree of research on GAN model. Aiming at the hot model of GAN, first introduce the research status of GAN; then introduce the theory and framework of GAN, which analyzes in detail why the gradient disappears and the mode collapses during the training of GAN; then discussed some typical GAN improvement models, and summarized their theoretical improvements, advantages, limitations, application scenarios and implementation costs; Finally, the application results of GAN in data generation, image super-resolution, and image style conversion are shown, and the current challenges and future research directions of GAN are discussed.},
  keywords={Training;Representation learning;Analytical models;Mechatronics;Computational modeling;Superresolution;Generative adversarial networks;machine learning;unsupervised learning;generative adversarial networks},
  doi={10.1109/ITOEC49072.2020.9141685},
  ISSN={},
  month={June},}@INPROCEEDINGS{11058807,
  author={Rofena, Aurora and Manchia, Arianna and Piccolo, Claudia Lucia and Zobel, Bruno Beomonte and Soda, Paolo and Guarrasi, Valerio},
  booktitle={2025 IEEE 38th International Symposium on Computer-Based Medical Systems (CBMS)}, 
  title={Lesion-Aware Generative Artificial Intelligence for Virtual Contrast-Enhanced Mammography in Breast Cancer}, 
  year={2025},
  volume={},
  number={},
  pages={141-146},
  abstract={Contrast-Enhanced Spectral Mammography (CESM) is a dual-energy mammographic technique that improves lesion visibility through the administration of an iodinated contrast agent. It acquires both a low-energy image, comparable to standard mammography, and a high-energy image, which are then combined to produce a dual-energy subtracted image highlighting lesion contrast enhancement. While CESM offers superior diagnostic accuracy compared to standard mammography, its use entails higher radiation exposure and potential side effects associated with the contrast medium. To address these limitations, we propose Seg-CycleGAN, a generative deep learning framework for Virtual Contrast Enhancement in CESM. The model synthesizes high-fidelity dual-energy subtracted images from low-energy images, leveraging lesion segmentation maps to guide the generative process and improve lesion reconstruction. Building upon the standard CycleGAN architecture, Seg-CycleGAN introduces localized loss terms focused on lesion areas, enhancing the synthesis of diagnostically relevant regions. Experiments on the CESM@UCBM dataset demonstrate that Seg-CycleGAN outperforms the baseline in terms of PSNR and SSIM, while maintaining competitive MSE and VIF. Qualitative evaluations further confirm improved lesion fidelity in the generated images. These results suggest that segmentation-aware generative models offer a viable pathway toward contrast-free CESM alternatives.},
  keywords={Training;Image segmentation;Visualization;Accuracy;Generative AI;Pipelines;Breast cancer;Lesions;Standards;Image reconstruction;Generative AI;GANs;Cycle GAN;CESM;Breast Cancer;FFDM},
  doi={10.1109/CBMS65348.2025.00039},
  ISSN={2372-9198},
  month={June},}@INPROCEEDINGS{10139097,
  author={Vokhmintsev, A. V. and Khristodulo, O. I. and Polyakova, L. Y.},
  booktitle={2023 International Conference on Industrial Engineering, Applications and Manufacturing (ICIEAM)}, 
  title={Mapping System to Interprete a Fracture Zones in Seismic Data Based on an Autoencoder and Generative Adversarial Networks}, 
  year={2023},
  volume={},
  number={},
  pages={1003-1007},
  abstract={Our research was aimed at developing methods and algorithms for identifying and mapping fracture zones of the Jurassic and pre-Jurassic complexes based on seismic data and methods of machine learning and artificial intelligence. In this paper it were developed and tested: the feature selection algorithm based on an autoencoder for interpreting data on seismic slices with low intraclass and high interclass variability, as well as the algorithm for synthesizing new images based on generative adversarial networks and a seismic database. The experiments carried out have shown that the algorithms have good accuracy and speed of operation, which makes them potentially suitable for processing large volumes of seismic information. The proposed autoencoder for candidate images with a high recall value both for the search variant with the micro-averaging metric and for the search with the macroaveraging metric. The automatic interpretation of seismic data on seismic slices of the Salymskaya well was carried out. The system for processing, analyzing and classifying 3D seismic information was developed based on the proposed methods and algorithms. The optimal predictive model was tuned based on machine learning and artificial intelligence methods.},
  keywords={Training;Measurement;Analytical models;Machine learning algorithms;Computational modeling;Geology;Machine learning;mapping of fractured systems;seismic exploration;machine learning and artificial intelligence methods;auto-encoder;geological cross-section;generative coupling network},
  doi={10.1109/ICIEAM57311.2023.10139097},
  ISSN={},
  month={May},}@INPROCEEDINGS{11158296,
  author={Kukreja, Sanjay and Kumar, Tarun and Bharate, Vishal and Gadwe, Sweta and Dasgupta, Abhijit and Guha, Debashis},
  booktitle={2025 5th International Conference on Artificial Intelligence and Education (ICAIE)}, 
  title={Performance Enhancement of Agentic Retrieval Augmented Generation Using Relevance Generative Answering}, 
  year={2025},
  volume={},
  number={},
  pages={465-469},
  abstract={The aim of this research paper is to present a novel approach of using Relevance Generative Answering (RGA) in the trending field of Agentic Retrieval Augmented Generation (RAG). The paradigm shift in the RAG system by the introduction of Agentic RAG has opened a new research paradigm. The major issue of hallucination is overcome with the use of a traditional RAG system with some limitations like accuracy and relevance, lack of reasoning, the lost in the middle problem, etc. The Agentic RAG system attempts to address a few of these limitations. However, interpreting results based on the user's intent remains a significant area of research. This research aimed to understand user intent by introducing relevance detection block in the proposed architecture. Different performance metrics like precision, recall, F1 score, relevance, latency are used to validate the proposed approach. The results presented in this research reveal that the performance of the proposed system is much more relevant compared to agentic RAG system. For context and intent specific applications proposed framework suits well.},
  keywords={Measurement;Generative AI;Large language models;Data integrity;Retrieval augmented generation;Education;Performance metrics;Natural language processing;Cognition;Complexity theory;Artificial Intelligence;Agentic Retrieval Augmented Generation;Relevance Generative Answering;Generative AI;Large Language Model;Natural Language Processing},
  doi={10.1109/ICAIE64856.2025.11158296},
  ISSN={},
  month={May},}@ARTICLE{8892628,
  author={Li, Gaolei and Ota, Kaoru and Dong, Mianxiong and Wu, Jun and Li, Jianhua},
  journal={IEEE Transactions on Industrial Informatics}, 
  title={DeSVig: Decentralized Swift Vigilance Against Adversarial Attacks in Industrial Artificial Intelligence Systems}, 
  year={2020},
  volume={16},
  number={5},
  pages={3267-3277},
  abstract={Individually reinforcing the robustness of a single deep learning model only gives limited security guarantees especially when facing adversarial examples. In this article, we propose DeSVig, a decentralized swift vigilance framework to identify adversarial attacks in an industrial artificial intelligence systems (IAISs), which enables IAISs to correct the mistake in a few seconds. The DeSVig is highly decentralized, which improves the effectiveness of recognizing abnormal inputs. We try to overcome the challenges on ultralow latency caused by dynamics in industries using peculiarly designated mobile edge computing and generative adversarial networks. The most important advantage of our work is that it can significantly reduce the failure risks of being deceived by adversarial examples, which is critical for safety-prioritized and delay-sensitive environments. In our experiments, adversarial examples of industrial electronic components are generated by several classical attacking models. Experimental results demonstrate that the DeSVig is more robust, efficient, and scalable than some state-of-art defenses.},
  keywords={Deep learning;Computational modeling;Edge computing;Data models;Informatics;Robustness;5G mobile communication;Adversarial examples;deep learning;generative adversarial networks (GAN);industrial artificial intelligence systems (IAISs);mobile edge computing},
  doi={10.1109/TII.2019.2951766},
  ISSN={1941-0050},
  month={May},}@INPROCEEDINGS{11136568,
  author={Li, Mengying and Zhang, Kai},
  booktitle={2025 International Conference on Intelligent Computing and Knowledge Extraction (ICICKE)}, 
  title={Design and Development of StyleGAN and Recurrent Networks in Visual Art Creation}, 
  year={2025},
  volume={},
  number={},
  pages={1-7},
  abstract={The application of artificial intelligence (AI) in visual art creation has unlocked new dimensions in creativity, automation, and personalized artistic expression. Conventional methods of generating art—typically based on rule-based systems or fixed templates—fail to engage the complexity, emotional depth, and stylistic variety that typify contemporary works of art. These existing drawbacks commonly lead to stiff and dull outputs that never manage to express rich artistic intent. To solve this aforementioned drawback, the current research investigates a hybrid AI strategy that exploits the strengths of StyleGAN and Long Short-Term Memory (LSTM) networks. In the suggested framework StyleGAN, is utilized to produce high-resolution and visually appealing visual artworks through training with a curated collection of classical and modern works. The latent vectors from the output are further analyzed by LSTM networks to add sequential and thematic aspects, adding contextual and emotional richness to the created work of art. The system was strictly tested using both quantitative and qualitative techniques. The system attained an FID score of 8.2 and a user satisfaction rate of 96% and has proven effective in creating visually engaging and emotionally appealing AI-generated pieces of art.},
  keywords={Training;Knowledge engineering;Visualization;Art;Automation;Generative adversarial networks;Vectors;Complexity theory;Long short term memory;Creativity;artificial intelligence (ai);visual art creation;stylegan;ai-based art generation;generative adversarial networks (gans);long short-term memory (lstm)},
  doi={10.1109/ICICKE65317.2025.11136568},
  ISSN={},
  month={June},}@ARTICLE{11087693,
  author={Oh, Sejun},
  journal={IEEE Access}, 
  title={Integration of MATH41 and Generative AI in Pre-Service Mathematics Teacher Education: An Empirical Study on Lesson Design Competency}, 
  year={2025},
  volume={13},
  number={},
  pages={128959-128973},
  abstract={Generative artificial intelligence (AI) tools are rapidly transforming mathematics education by enabling automated problem generation, dynamic visualizations, and adaptive learning experiences. This study presents an empirical study on incorporating MATH41 into a 15-week pre-service teacher preparation course for mathematics majors. Twenty-four participants learned to create parameterized math problems, generate vector graphics. Following guided training, each participant designed and micro-taught a 20-minute lesson incorporating MATH41-generated resources. Reflection journals and final lesson plans were analyzed thematically, while 20 participants completed a self-assessment survey on lesson design competency. Results reveal that the automated problem generation capabilities motivated pre-service teachers to explore a broader range of instructional strategies, including personalized tasks and diverse problem variants. Reflection data indicate that while integrating AI tools can significantly boost confidence and creativity in lesson planning, careful pedagogical alignment remains essential to avoid superficial learning. Participants underscored the importance of maintaining teacher oversight—especially when adapting AI-generated problems for particular learner needs. Additionally, their post-course self-assessments showed high confidence in digital tool integration, yet they acknowledged that anticipating student misconceptions requires further field experience. Overall, this study contributes to understanding how teacher education programs can enhance lesson design competencies via structured AI tool integration. It also highlights the critical role of reflective practice in ensuring that automated content creation fosters deeper instructional effectiveness rather than uncritical AI dependency.},
  keywords={Artificial intelligence;Education;Generative AI;Planning;Mathematics;Problem-solving;Reflection;Chatbots;Training;Ethics;Generative AI;mathematics education;pre-service teachers;MATH41;problem generation;instructional design competency;teacher reflection;AI in education;lesson planning},
  doi={10.1109/ACCESS.2025.3586593},
  ISSN={2169-3536},
  month={},}@INBOOK{10953983,
  author={Baker, Pam},
  booktitle={Generative AI For Dummies}, 
  title={Navigating the Evolving Landscape of GenAI}, 
  year={2025},
  volume={},
  number={},
  pages={49-61},
  abstract={Summary <p>This chapter discusses the core concepts and shows how to identify the key players who are shaping the generative artificial intelligence (GenAI) landscape. Many GenAI services offer a free version or a free trial. The integration of AI into existing software ecosystems isn't just a luxury &#x2014; it's a necessity. Autonomous AI agents are digital dynamos capable of tackling tasks from content creation to personal finance and transportation bookings, all on their own. The GenAI field is led by a vanguard of innovative companies, from tech titans like OpenAI and Google to lesser&#x2010;known startups that are redefining the boundaries of what's possible. The backbone of GenAI innovation lies in the rigorous academic research that propels it. The GenAI community is a melting pot of ideas, debates, and collaborative problem&#x2010;solving. As GenAI applications become more sophisticated, the need for robust and scalable infrastructure becomes increasingly important.</p>},
  keywords={Artificial intelligence;Internet;Companies;Generative AI;Faces;Technological innovation;Navigation;Biological system modeling;Stability analysis;Computational modeling},
  doi={},
  ISSN={},
  publisher={Wiley},
  isbn={9781394270767},
  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/10953983},}@INPROCEEDINGS{10964965,
  author={Kodmalwar, Pratik and Kapse, Leena and Mashankar, Amol and Mansukhani, Kumar and Mahalle, Amruta and Bhosale, Nilesh},
  booktitle={2024 2nd International Conference on Emerging Trends in Engineering and Medical Sciences (ICETEMS)}, 
  title={Testing the Accuracy of Fibonacci Retracement Levels Using Artificial Intelligence}, 
  year={2024},
  volume={},
  number={},
  pages={399-404},
  abstract={During the last few years the Indian stock market has witnessed an overwhelmed participation of new investors and the 42 lakh DEMAT Account openings in 2023 quoted by Economic Times is a prime evidence of that. Considering the rising inclination of Euphoria towards the stock market, arming oneself with the knowledge of technical analysis has become the need of the hour. In intraday and Swing Trading, taking a right entry and exit is the most crucial thing in order to make agood amount of profit and that ultimately showcases the importance of identifying a trend reversal. One of the most popular indicator for identifying a trend reversal is Fibonacci Retracement. Fibonacci Retracement is based on Fibonacci sequence and it helps a trader in plotting the support and resistance levels. Purpose and Focus - There are many research papers explaining how Fibonacci retracement works and how to use that but there is no literature that showcases the empirical evidence that demonstrates the accuracy of Fibonacci retracement in identifying the trend reversals. Back testing of one small cap, mid cap and large cap stock listed on NSE will be done on different time frames using Artificial Intelligence and trading view to test the accuracy and effectiveness of Fibonacci retracement.},
  keywords={Resistance;Accuracy;Generative AI;Back;Market research;Encoding;Reliability;Stock markets;Testing;Artificial Intelligence;Fibonacci Retracement;Trend Reversal;Back-testing;Pine Script},
  doi={10.1109/ICETEMS64039.2024.10964965},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10671149,
  author={Yi, Jiachen and Wu, Junshuo and Liu, Qian and Zhou, Chutong and Jia, Yuanyuan and Wang, Yuchen},
  booktitle={2024 3rd International Conference on Robotics, Artificial Intelligence and Intelligent Control (RAIIC)}, 
  title={Research on the Classification and Planning of Functional Areas in Smart Cities Based on Artificial Intelligence}, 
  year={2024},
  volume={},
  number={},
  pages={399-402},
  abstract={This study presents an innovative approach for urban functional zoning and planning based on artificial intelligence (AI) techniques. Utilizing multi-modal remote sensing data and geographic information, this research proposes a deep learning framework to accurately classify and segment urban functional areas. The proposed method leverages a Transformer-based Spatio-Temporal Fusion Network (TBST) that integrates high-resolution satellite imagery and time-series population data. The TBST model comprises two feature extraction branches, ResMixer and PDNet, which respectively handle spatial and temporal features, followed by a Transformer-based adaptive fusion layer to enhance the extraction of informal settlements in urban areas. Experimental results demonstrate that the multimodal data fusion significantly improves the accuracy of urban functional area classification, achieving superior performance metrics compared to single-modal approaches. This method holds significant potential for advancing smart city planning by providing precise and up-to-date spatial distribution information, thus supporting urban policy-making and sustainable development.},
  keywords={Adaptation models;Accuracy;Smart cities;Urban planning;Feature extraction;Transformers;Data models;component;Smart City Planning;Urban Functional Zoning;Multi-modal Data Fusion;Deep Learning},
  doi={10.1109/RAIIC61787.2024.10671149},
  ISSN={},
  month={July},}@INPROCEEDINGS{9894338,
  author={Suo, Kun and Nguyen, Tu N. and Shi, Yong and He, Jing Selena and Hung, Chih-Cheng},
  booktitle={2022 IEEE International Performance, Computing, and Communications Conference (IPCCC)}, 
  title={Keep Clear of the Edges : An Empirical Study of Artificial Intelligence Workload Performance and Resource Footprint on Edge Devices}, 
  year={2022},
  volume={},
  number={},
  pages={7-16},
  abstract={Recently, with the advent of the Internet of everything and 5G network, the amount of data generated by various edge scenarios such as autonomous vehicles, smart industry, 4K/8K, virtual reality (VR), augmented reality (AR), etc., has greatly exploded. All these trends significantly brought real-time, hardware dependence, low power consumption, and security requirements to the facilities, and rapidly popularized edge computing. Meanwhile, artificial intelligence (AI) workloads also changed the computing paradigm from cloud services to mobile applications dramatically. Different from wide deployment and sufficient study of AI in the cloud or mobile platforms, AI workload performance and their resource impact on edges have not been well understood yet. There lacks an in-depth analysis and comparison of their advantages, limitations, performance, and resource consumptions in an edge environment. In this paper, we perform a comprehensive study of representative AI workloads on edge platforms. We first conduct a summary of modern edge hardware and popular AI workloads. Then we quantitatively evaluate three categories (i.e., classification, image-to-image, and segmentation) of the most popular and widely used AI applications in realistic edge environments based on Raspberry Pi, Nvidia TX2, etc. We find that interaction between hardware and neural network models incurs non-negligible impact and overhead on AI workloads at edges. Our experiments show that performance variation and difference in resource footprint limit availability of certain types of workloads and their algorithms for edge platforms, and users need to select appropriate workload, model, and algorithm based on requirements and characteristics of edge environments.},
  keywords={Solid modeling;Cloud computing;Power demand;Image edge detection;Neural networks;Hardware;Real-time systems;edge computing;artificial intelligence;performance;resource footprint},
  doi={10.1109/IPCCC55026.2022.9894338},
  ISSN={2374-9628},
  month={Nov},}@INPROCEEDINGS{9831206,
  author={Liu, Zhijuan and Zhang, Li and Wu, Xuangou and Zhao, Wei},
  booktitle={2022 IEEE 23rd International Conference on High Performance Switching and Routing (HPSR)}, 
  title={Test Case Filtering based on Generative Adversarial Networks}, 
  year={2022},
  volume={},
