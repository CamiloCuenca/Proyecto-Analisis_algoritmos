  ISSN={2381-8549},
  month={Sep.},}@INPROCEEDINGS{11100767,
  author={Chan, Yuet Hung Cecilia},
  booktitle={2025 6th International Conference on Information Technology and Education Technology (ITET)}, 
  title={Exploring Language, Society, and Culture with AI Video Creation: A Hands-On Case Study}, 
  year={2025},
  volume={},
  number={},
  pages={6-10},
  abstract={This case study aims to evaluate the impact of AI video creation tools on enhancing students’ interest, engagement, and ability to conduct investigations related to language, society, and culture. The participants were university students enrolled in a sociolinguistics course. They received training on using AI video creation tools to create short videos for their sociolinguistic projects. After completing the projects, the students were invited to complete a survey to assess the extent to which the case study achieved its objectives. The survey included seven factors: self-efficacy, actual usage, perceived ease of use, perceived usefulness, user attitude, perceived learning outcomes, and intention for future use. The correlation test results show that all these factors were significantly and positively correlated, and most correlations were strong. The survey reveals that AI video creation tools enhance students’ interest and engagement in their sociolinguistic investigations and improve their creativity, problem-solving, critical thinking, and communication skills. The positive survey results, along with the informative and creative videos generated by the students, demonstrate the applicability of AI technology in education.},
  keywords={Surveys;Training;Correlation;Generative AI;Problem-solving;Artificial intelligence;Information technology;Creativity;Videos;Generative AI;AI-generated videos;sociolinguistic investigations;AI technology in education},
