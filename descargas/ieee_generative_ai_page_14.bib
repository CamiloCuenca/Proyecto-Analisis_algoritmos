  month={June},}@INPROCEEDINGS{10885478,
  author={Chang, Chin-Chih and Wei, Chi-Hung and Hsiao, Sean and Yang, Chyuan-Huei Thomas},
  booktitle={2024 IEEE 6th Eurasia Conference on Biomedical Engineering, Healthcare and Sustainability (ECBIOS)}, 
  title={Meal Image Recognition and Healthy Meal Combination Recommendation System Integrated with Generative Artificial Intelligence}, 
  year={2024},
