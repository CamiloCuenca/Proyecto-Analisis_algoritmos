  abstract={In order to estimate the model using the method it is necessary to train two different models simultaneously. The first model, called the model (D) is responsible, for determining whether a particular sample is from the training data or generated by another model called G. On the hand G aims to understand the underlying distribution of the data. By training G to maximize the likelihood of D making mistakes a competitive environment is established. There are applications for G and D but one particularly notable solution emerges. D consistently evaluates any input as if it came from training data while G effectively captures the details of that training data distribution. Notably this system can be trained using known techniques like backpropagation when implementing G and D as multilayer perceptrons. This eliminates the need for inference networks or intricate Markov chains, during both training and sample generation processes. Thorough evaluations of the generated samples confirm the potential of this framework as demonstrated through testing. This innovative approach offers an efficient means to capture data distributions, which could potentially revolutionize generative modeling.},
  keywords={Training;Image synthesis;Computational modeling;Training data;Generative adversarial networks;Data models;Task analysis;Deep learning;Machine learning algorithms;Image classification;Synthetic image generation;Generative Adversarial Network (GAN);Generator and discriminator;Image synthesis Image generation},
  doi={10.1109/ICCSAI59793.2023.10421513},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10463222,
  author={Yao, Yifan and Shin, Juin and Jin, Xianglan},
  booktitle={2024 International Conference on Artificial Intelligence in Information and Communication (ICAIIC)}, 
  title={End-to-End MIMO Systems with Conditional Generative Adversarial Networks}, 
  year={2024},
  volume={},
  number={},
  pages={480-483},
  abstract={Conventional autoencoder-based systems, comprising a neural-network encoder at the transmitter and a neural-network decoder at the receiver, often face limitations in re-alistic signal transmission due to its reliance on differentiable channel models. In response to this limitation, an end-to-end framework has emerged, employing a conditional generative adversarial network (CGAN) for channel learning. The CGAN not only learns to represent channel effects through feature extraction but also facilitates gradient back-propagation between the receiver and the transmitter, thereby enhancing the system's adaptability. This paper extends the CGAN-based end-to-end system from single input single output channels to multiple input multiple output (MIMO) scenarios. This CGAN-based MIMO system demonstrates promising performance comparable to the autoencoder communication systems, highlighting the potential of CGANs as effective alternatives for original channels.},
  keywords={Transmitters;Simulation;Receivers;Rayleigh channels;Generative adversarial networks;Feature extraction;Decoding;Autoencoder;deep learning;generative adversarial network (GAN);multiple input multiple output (MIMO)},
  doi={10.1109/ICAIIC60209.2024.10463222},
  ISSN={2831-6983},
  month={Feb},}@INPROCEEDINGS{10911646,
  author={Chen, Zhongtao and Yao, Yanli and Li, Xinyi},
  booktitle={2024 6th International Conference on Robotics, Intelligent Control and Artificial Intelligence (RICAI)}, 
  title={Photovoltaic Random Scenario Prediction Based on Time Generative Adversarial Networks}, 
  year={2024},
  volume={},
  number={},
  pages={595-600},
  abstract={Due to the randomness and time correlation of photovoltaic (PV) power output, the PV data obtained by conventional short-term point or interval prediction methods cannot support the robust scheduling and reliable operation of the power system under the condition of high proportion of PV integration. A photovoltaic random scenario prediction model based on Time - generative adversarial network (TimeGAN) is proposed. Firstly, the photovoltaic samples are decomposed into static and time-series data, and the network structures of embedding, reconstruction, generation and discrimination are constructed. Secondly, taking the static and time-series data as samples, the embedding and reconstruction networks are jointly trained. Finally, taking the output of the embedding network as samples, the generation and discrimination networks are trained adversarially, and the output of the generation network is fed back to the embedding-reconstruction training to test the fitting effect of the time series. Aiming to minimize the reconstruction, supervised and unsupervised losses, the model conducts in-depth learning of the photovoltaic time characteristics and predicts random scenarios. The validity of the proposed model is verified by the example calculation.},
  keywords={Photovoltaic systems;Analytical models;Correlation;Predictive models;Wind power generation;Power system stability;Generative adversarial networks;Data models;Power system reliability;Reliability;Photovoltaic Power Output;Random Scenario Prediction;Time - generative adversarial network (TimeGAN)},
  doi={10.1109/RICAI64321.2024.10911646},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10674442,
  author={Gao, Yuquan and Sun, Guoxi and Zhao, Xinzhuo},
  booktitle={2024 IEEE 4th International Conference on Software Engineering and Artificial Intelligence (SEAI)}, 
  title={Single Face Image Super-Resolution Reconstruction with Wasserstein Generative Adversarial Networks}, 
  year={2024},
  volume={},
  number={},
  pages={63-67},
  abstract={Face image super-resolution reconstruction is a crucial task for enhancing low-quality surveillance videos. Most state-of-the-art approaches employ generative adversarial networks (GANs). However, GANs are known to suffer from training instability issues. We propose a single face image super-resolution method called Single Face Image Super-Resolution Reconstruction (SFSR), based on Wasserstein GANs and residual dense networks. Without requiring any prior information, our model can directly generate high-resolution face images at 4x or 8x scale from arbitrary low-resolution inputs, outperforming traditional bilinear upsampling. To further enhance image quality, we utilize a multi-level residual dense generator with skip connections. Our loss function combines pixel-wise, perceptual, and adversarial losses to simultaneously optimize fidelity and realism. Extensive experiments on CelebA and CNBC datasets demonstrate state-of-the-art quantitative and qualitative performance, validating the efficacy of our approach for stabilizing GAN training and producing sharp, artifact-free super-resolved face images.},
  keywords={Training;Technological innovation;Surveillance;Superresolution;Noise;Generative adversarial networks;Faces;Super-resolution;Wasserstein Generative Adversarial Networks;Residual Dense Networks;Face images},
  doi={10.1109/SEAI62072.2024.10674442},
  ISSN={},
  month={June},}@INPROCEEDINGS{11116943,
  author={Zou, Yijie and Fang, Guangxian},
  booktitle={2024 5th International Conference on Big Data, Artificial Intelligence and Internet of Things Engineering (ICBAIE)}, 
  title={Underwater Image Enhancement Based on Generative Adversarial Network with Multi-Attention}, 
  year={2024},
  volume={},
  number={},
  pages={351-354},
  abstract={Underwater image enhancement is a crucial technology for supporting underwater exploration and rescue operations. With the continuous development of the ocean, underwater activities such as archaeology, pipeline inspection, and dredging have been increasing. Nevertheless, owing to the unique complexity of the underwater environment, captured underwater images often face the challenge of information loss, manifested as blurry images, low signal - to - noise ratios, and poor color quality. These issues significantly limit the effective utilization and value of high - quality underwater images in scientific research and engineering practices. Currently, common problems in the field of underwater image enhancement research include high algorithm complexity and insufficient feature extraction capabilities. Therefore, to address the problems of insufficient feature extraction of the target information under test, we adopted the generative adversarial network (GAN) as the main architecture and incorporated an attention mechanism into the network generator. This enables us to centre around the key information of the image during feature extraction, resulting in clearer images. Extensive experiments demonstrate that our method outperforms existing underwater image enhancement methods, validating the effectiveness and the proposed approach.},
  keywords={Image color analysis;Oceans;Pipelines;Inspection;Generative adversarial networks;Feature extraction;Generators;Complexity theory;Internet of Things;Image enhancement;Image Underwater image enhancement;attention;generative adversarial network},
  doi={10.1109/ICBAIE63306.2024.11116943},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10868713,
  author={Zhou, Qiang and Jia, Junxia},
  booktitle={2024 IEEE 4th International Conference on Information Technology, Big Data and Artificial Intelligence (ICIBA)}, 
  title={Multi-scale Fusion Dehazing Algorithm based on Generative Adversarial Network}, 
  year={2024},
  volume={4},
  number={},
  pages={1350-1356},
  abstract={In haze weather, visible light is scattered and absorbed when it passes through the atmosphere, causing significant degradation in image quality. To address this issue, we propose a multi-scale fusion dehazing algorithm based on an improved Generative Adversarial Network (GAN). In the generator part, we design a multi-scale fusion attention network (Grid-G) with a three rows, multi-column structure. The network first extracts and strengthens local and global features in the image through a feature enhancement module, effectively solving the problem that traditional dehazing methods cannot fully capture image details and global semantic information. Then, channel attention and coordinate attention are introduced to process the dense fog area and high-frequency area of the image from different angles. Finally, a multi-scale fusion module is designed to replace the original ordinary feature splicing operation and enhance the detail fidelity of the image.In the discriminator part, a fusion discriminator (FDG-D) is constructed to enhance the ability to distinguish the source of the image by introducing the high-frequency and low-frequency information of the image as additional priors. The experimental results show that the algorithm proposed in this paper not only has natural colors and clear details subjectively, but also outperforms the existing mainstream algorithms in objective indicators.},
  keywords={Image color analysis;Splicing;Semantics;Generative adversarial networks;Feature extraction;Generators;Image restoration;Visual perception;Synthetic data;Meteorology;image dehazing;Generative Adversarial Network;multi-scale fusion;fusion discriminator},
  doi={10.1109/ICIBA62489.2024.10868713},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{9696122,
  author={Chen, Yijia},
  booktitle={2021 2nd International Conference on Big Data & Artificial Intelligence & Software Engineering (ICBASE)}, 
  title={Speech Generation by Generative Adversarial Network}, 
  year={2021},
  volume={},
  number={},
  pages={435-438},
  abstract={This paper takes a review of some recent state-of-the-art works in the field of speech generation through adopting adaptive Generative Adversarial Network (GAN). It firstly introduces what is GAN model and takes an overview of fields of applications. It then presents the theoretical foundations of this model, advantages, and disadvantages. After this, it reviews research works of Voice Conversion (VC) using GAN in the type of parallel framework and non-parallel framework. In addition, it provides readers with a more specific comparison between many of these works, in particular non-parallel ones. Finally, we propose future directions: research work might focus on a non-parallel framework and might especially focus on achieving sequence-to-sequence voice translations.},
  keywords={Adaptation models;Adaptive systems;Generative adversarial networks;Mathematical models;Software engineering;Non-parallel Voice Conversion (VC);Generative Adversarial Network (GAN)},
  doi={10.1109/ICBASE53849.2021.00086},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10868955,
  author={Wen, Yana and Meng, Jiaying},
  booktitle={2024 IEEE 4th International Conference on Information Technology, Big Data and Artificial Intelligence (ICIBA)}, 
  title={Diversified Generation and Evaluation of English Cultural Communication Materials Based on Generative Adversarial Networks (GANs)}, 
  year={2024},
  volume={4},
  number={},
  pages={636-640},
  abstract={English has become the most widely used language in the world. In the fields of English education and cultural dissemination, traditional teaching methods and materials are no longer able to meet the growing demand for cultural understanding and communication. This study constructed a Generative Adversarial Networks (GANs) model, which generates highly realistic and diverse cultural communication materials by training generators and discriminators. The generator generates data from random noise, while the discriminator distinguishes between generated data and real data. The experimental results show that the Fréchet Inception Distance (FID) value of the material generated by GANs is much lower than that of Convolutional Neural Network (CNN) and Recurrent Neural Network (RNN), exhibiting higher image realism; its Mode Coverage (MC) value is also higher than other methods, averaging 90.428%. In addition, the audience showed high click through rates and engagement with the materials generated by GANs, verifying their effectiveness in cultural communication and confirming the potential of GANs in generating diverse English cultural communication materials.},
  keywords={Training;Recurrent neural networks;Noise;Generative adversarial networks;Generators;Cultural differences;Global communication;Convolutional neural networks;Security;Information technology;Generative Adversarial Network;English Cultural Dissemination;Convolutional Neural Network;Recurrent Neural Network},
  doi={10.1109/ICIBA62489.2024.10868955},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{9065067,
  author={Moussa, Ahmad and Watanabe, Hiroshi},
  booktitle={2020 International Conference on Artificial Intelligence in Information and Communication (ICAIIC)}, 
  title={Audio Translation with Conditional Generative Adversarial Networks}, 
  year={2020},
  volume={},
  number={},
  pages={438-442},
  abstract={This paper explores the applicability of conditional generative adversarial networks in audio-to-audio translation problems and proposes a neural network architecture capable of doing so. Recent advances have shown that causal convolutions can be effective for modeling raw audio when their kernel is dilated by many factors, in contrast to previous techniques that utilized recurrent approaches. Embedding such convolutions within a conditional GAN architecture allows the targeted generation of raw audio given a certain input. This architecture can then be used to learn and simulate certain translative operations applied to an input signal. This creates the defined problem of converting one audio signal into another, which has different characteristics. We also propose a novel discriminator structure for the evaluation of generated audio.},
  keywords={Generators;Convolutional codes;Convolution;Decoding;Training;Generative adversarial networks;Computer architecture;conditional generative adversarial networks;causal dilated convolutions;audio effects;signal processing},
  doi={10.1109/ICAIIC48513.2020.9065067},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10137839,
  author={Wu, Zhuohao and Li, Yanni and Ji, Danwen and Wu, Dingming and Shidujaman, Mohammad and Zhang, Yuan and Zhang, Chenfan},
  booktitle={2022 6th Asian Conference on Artificial Intelligence Technology (ACAIT)}, 
  title={Human-AI Co-Creation of Art Based on the Personalization of Collective Memory}, 
  year={2022},
  volume={},
  number={},
  pages={1-6},
  abstract={Artificial intelligence (AI) is trained with data, especially texts, numbers, images, videos and music on Internet. These data all together across time and space make a collective memory of the world. The latest large-scale AI models give people a chance to create out of a large pool of this collective memory, which they won’t be able to access before, and communicate with AI in both human natural language and the unique machine supported ways. As demonstrated and discussed in this paper, effective and efficient workflows can be built up for human and AI to co-create meaningful results based on both the collective memory of the world and the personalized ideas and tastes. This kind of human-AI co-creation has a new force with great potential, and expect a new strategy and philosophy to guide human-AI collaboration.},
  keywords={Philosophical considerations;Art;Memory management;Natural languages;Force;Collaboration;Internet;creativity;artificial intelligence;AI;art;design;methods;process management},
  doi={10.1109/ACAIT56212.2022.10137839},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10962979,
  author={Jayaweera, I.G.U.D},
  booktitle={2025 5th International Conference on Advanced Research in Computing (ICARC)}, 
  title={Revolutionizing Education: Generative AI as a Catalyst for Personalized Learning and Innovative Teaching Practices}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Generative AI is transforming education by providing innovative tools for personalized learning, content creation, and teaching assistance. This paper explores the potential of generative AI in enhancing teaching methodologies and learning experiences, addressing challenges, and proposing solutions for its effective integration into education systems. The generative AI tool, ChatGPT, has captivated global attention with its remarkable ability to perform sophisticated and complex tasks. Its advanced capabilities, particularly within the field of education, have elicited mixed reactions among educators, as this breakthrough in AI technology is poised to revolutionize traditional educational practices. This exploratory study synthesizes recent literature to examine the potential benefits and challenges of ChatGPT in enhancing teaching and learning. On the positive side, ChatGPT offers opportunities to foster personalized and interactive learning experiences, support the creation of prompts for formative assessments, and provide ongoing feedback to inform teaching and learning processes. However, the study also identifies significant limitations inherent to ChatGPT, such as the generation of inaccurate information, potential biases stemming from its training data, and privacy concerns that may exacerbate existing inequities. To ensure the constructive and safe integration of ChatGPT into education, the study presents recommendations for its effective use. These include encouraging collaboration among policymakers, researchers, educators, and technology experts to address the risks and ethical implications of generative AI. By initiating proactive discussions and developing comprehensive guidelines, stakeholders can harness the potential of ChatGPT to enhance education while safeguarding the interests of learners and educators alike.},
  keywords={Ethics;Data privacy;Adaptive learning;Generative AI;Education;Collaboration;Training data;Chatbots;Stakeholders;Guidelines;ChatGPT;Generative AI;Graduate Education;Personalized Learning;Teaching and Learning},
  doi={10.1109/ICARC64760.2025.10962979},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10674838,
  author={Li, Panfeng and Yang, Qikai and Geng, Xieming and Zhou, Wenjing and Ding, Zhicheng and Nian, Yi},
  booktitle={2024 5th International Conference on Electronic Communication and Artificial Intelligence (ICECAI)}, 
  title={Exploring Diverse Methods in Visual Question Answering}, 
  year={2024},
  volume={},
  number={},
  pages={681-685},
  abstract={This study explores innovative methods for improving Visual Question Answering (VQA) using Generative Adversarial Networks (GANs), autoencoders, and attention mechanisms. Leveraging a balanced VQA dataset, we investigate three distinct strategies. Firstly, GAN-based approaches aim to generate answer embeddings conditioned on image and question inputs, showing potential but struggling with more complex tasks. Secondly, autoencoder-based techniques focus on learning optimal embeddings for questions and images, achieving comparable results with GAN due to better ability on complex questions. Lastly, attention mechanisms, incorporating Multimodal Compact Bilinear pooling (MCB), address language priors and attention modeling, albeit with a complexity-performance trade-off. This study underscores the challenges and opportunities in VQA and suggests avenues for future research, including alternative GAN formulations and attentional mechanisms.},
  keywords={Visualization;Attention mechanisms;Costs;Computational modeling;Refining;Generative adversarial networks;Question answering (information retrieval);Visual Question Answering;Generative Adversarial Networks;Autoencoders;Attention},
  doi={10.1109/ICECAI62591.2024.10674838},
  ISSN={},
  month={May},}@INPROCEEDINGS{10577712,
  author={Silva, Adriano},
  booktitle={2024 International Conference on Smart Applications, Communications and Networking (SmartNets)}, 
  title={Did ChatGPT Exterminate the Writer, Translator, Reviewer and Journalist Professions in Brazil?}, 
  year={2024},
  volume={},
  number={},
  pages={1-4},
  abstract={The research led by AI specialist, Andre Cia, stated that ChatGPT would immediately extinguish the profession of copywriter, translator, and proofreader and, within 12 months, the profession of journalist. The eventual extinction of such professions, through the transfer of imminently human tasks to a Generative AI technology, will increase structural unemployment in Brazil, due to the need for these unemployed workers to seek greater qualifications so that they can obtain their reinsertion into the job market. The purpose of this article is to analyze and determine whether the exterminating predictions mentioned were materialized, including the worsening of Brazilian structural unemployment, with repercussions on public spending for the payment of unemployment insurance. Using the quantitative research method to approach numerical data and public statistics on the subject, as well as descriptive qualitative for the purpose of understanding the phenomenon under study and the technique of reviewing accessible literature, supporting literature and reviewing the legislation, it was found that the use of ChatGPT did not exterminate the professions studied and was not even responsible for the high Brazilian unemployment rate or high public spending.},
  keywords={Generative AI;Insurance;Legislation;Chatbots;Unemployment;Task analysis;Qualifications;ChatGPT;Extinction of the role of editor;translator;proofreader;and journalist;Structural Unemployment;Generative artificial intelligence (AI);unemployment insurance},
  doi={10.1109/SmartNets61466.2024.10577712},
  ISSN={2837-4940},
  month={May},}@INPROCEEDINGS{10386946,
  author={Bushey, Jessica},
  booktitle={2023 IEEE International Conference on Big Data (BigData)}, 
  title={AI-Generated Images as an Emergent Record Format}, 
  year={2023},
  volume={},
  number={},
  pages={2020-2031},
  abstract={AI-generated Images are disrupting existing approaches to verifying the trustworthiness of visual media. The application of generative AI in fields in which images are trusted visual evidence of persons, actions and events is drawing the attention of archival scientists and AI researchers. A literature review of AI-generated images as an emergent record format, identified an absence of archival and recordkeeping knowledge. Analysis of the results revealed six thematic categories: authenticity and verifiability; manipulation and misinformation; bias and representation; attribution and intellectual property; transparency and explainability; and ethical considerations. These themes inform the development of research questions and the next phase of the study that includes the application of theory and methods of archival diplomatics and computational archival science.},
  keywords={Visualization;Ethics;Generative AI;Bibliographies;Intellectual property;Big Data;Fake news;Generative-AI;AI-generated images;Archives;Recordkeeping;Computational Archival Science;Born-digital images},
  doi={10.1109/BigData59044.2023.10386946},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10455994,
  author={Singh, Vaishali and S, Veena C and N, Raja Praveen K},
  booktitle={2023 IEEE International Conference on ICT in Business Industry & Government (ICTBIG)}, 
  title={Secure and Tailored Health Solutions for People with Specific Requirements Leveraging Advanced AI Techniques}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={Our proposed method aims to revolutionize healthcare by leveraging advanced AI techniques to tailor health solutions for individuals with specific health requirements. This innovative approach integrates three powerful AI algorithms: Deep Neural Network (DNN), Long Short-Term Memory (LSTM), and Generative Adversarial Network (GAN). These algorithms work collaboratively to develop secure, precise, and individualized healthcare services. The DNN mimics the brain's structure to extract intricate patterns, the LSTM handles sequential data and long-term dependencies, and the GAN generates data resembling real distributions. Security and privacy are ensured through data encryption and secure communication. The tailored health solution is a result of integrating these techniques, providing enhanced healthcare services.},
  keywords={Support vector machines;Measurement;Medical services;Generative adversarial networks;Real-time systems;Safety;Security;AI techniques;Data Fusion;Deep Neural Network;Generative Adversarial Network;Long Short-Term Memory;Secure Communication},
  doi={10.1109/ICTBIG59752.2023.10455994},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10701449,
