  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/10950694},}@INPROCEEDINGS{11051607,
  author={Liu, Weipeng and Bao, Runchen and Wang, Yuan and Zhu, Haixing and Yang, Hongwei and Zhang, Jiaqi},
  booktitle={2025 Joint International Conference on Automation-Intelligence-Safety (ICAIS) & International Symposium on Autonomous Systems (ISAS)}, 
  title={Multi-scale adversarial generative network for super-resolution of endoscopic images}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={High-definition endoscopic images can provide clinicians with accurate marginal information and complex lesion texture details. However, in endoscopic surgery, the image resolution is reduced due to the limited resolution of the acquisition camera, the imaging noise introduced by the endoscopic light source, and the physiological movement that causes motion artifacts in the texture of the lesion edge. To address these issues, a super-resolution reconstruction method based on conditional adversarial generative networks is proposed. To tackle the problem of blurred edge textures, a multi-scale mixed attention feature extraction module is designed in the down sampling stage, which extracts image features and enhances the weight of local areas. In the up sampling stage, high-frequency details extracted from the Laplacian pyramid are introduced. To effectively remove light source noise and improve the reconstruction effect of edge textures, a novel hybrid perceptual function is proposed that fuses content loss, style loss, total variation loss, perceptual loss, and texture loss based on L1 loss and adversarial generation loss. The discriminator uses a relative discriminator to enhance the quality of the generated images by judging how much more realistic the real images are compared to the generated ones. The reconstruction results on the Kvasir dataset and a private dataset show that the PSNR index increases by an average of 5.34 dB, the SSIM index increases by 0.10 on average, the PIQM decreases by 0.015 on average, and the LPIPS decreases by 0.074 on average, indicating that the proposed method demonstrates significant advantages in detail representation and lesion texture reconstruction, and can effectively suppress noise to improve overall visual quality.},
  keywords={Training;Visualization;Image edge detection;Superresolution;Noise;Feature extraction;Lesions;Image reconstruction;Light sources;Motion artifacts;Conditional generation adversarial networks;Multi-scale mixed attention mechanism;Pyramid of Laplace;Super-resolution reconstruction},
  doi={10.1109/ICAISISAS64483.2025.11051607},
  ISSN={2996-3850},
  month={May},}@ARTICLE{10226350,
  author={Gao, Xingyu and Liu, Hongrui and Shi, Feng and Shen, Dinggang and Liu, Manhua},
  journal={IEEE Journal of Biomedical and Health Informatics}, 
  title={Brain Status Transferring Generative Adversarial Network for Decoding Individualized Atrophy in Alzheimer's Disease}, 
  year={2023},
  volume={27},
  number={10},
  pages={4961-4970},
  abstract={Deep learning has been widely investigated in brain image computational analysis for diagnosing brain diseases such as Alzheimer's disease (AD). Most of the existing methods built end-to-end models to learn discriminative features by group-wise analysis. However, these methods cannot detect pathological changes in each subject, which is essential for the individualized interpretation of disease variances and precision medicine. In this article, we propose a brain status transferring generative adversarial network (BrainStatTrans-GAN) to generate corresponding healthy images of patients, which are further used to decode individualized brain atrophy. The BrainStatTrans-GAN consists of generator, discriminator, and status discriminator. First, a normative GAN is built to generate healthy brain images from normal controls. However, it cannot generate healthy images from diseased ones due to the lack of paired healthy and diseased images. To address this problem, a status discriminator with adversarial learning is designed in the training process to produce healthy brain images for patients. Then, the residual between the generated and input images can be computed to quantify pathological brain changes. Finally, a residual-based multi-level fusion network (RMFN) is built for more accurate disease diagnosis. Compared to the existing methods, our method can model individualized brain atrophy for facilitating disease diagnosis and interpretation. Experimental results on T1-weighted magnetic resonance imaging (MRI) data of 1,739 subjects from three datasets demonstrate the effectiveness of our method.},
  keywords={Brain modeling;Pathology;Magnetic resonance imaging;Generators;Generative adversarial networks;Data models;Bioinformatics;Alzheimer's disease;generative adversa- rial network;generative model;individualized diagnosis;T1-weighted magnetic resonance imaging},
  doi={10.1109/JBHI.2023.3304388},
  ISSN={2168-2208},
  month={Oct},}
