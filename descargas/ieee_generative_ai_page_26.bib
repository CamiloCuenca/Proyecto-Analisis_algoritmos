  ISSN={},
  month={March},}@INBOOK{10950694,
  author={Heilig, Thorsten and Scheer, Ilhan},
  booktitle={Decision Intelligence: Transform Your Team and Organization with AI-Driven Decision-Making}, 
  title={The Technology Stack: Applying AI Systems for Decision&#x2010;Making}, 
  year={2024},
