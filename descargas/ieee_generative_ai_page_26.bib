  booktitle={2023 4th International Conference for Emerging Technology (INCET)}, 
  title={Conversational Chatbot Builder – Smarter Virtual Assistance with Domain Specific AI}, 
  year={2023},
  volume={},
  number={},
  pages={1-4},
  abstract={Artificial intelligence (AI) has the potential to revolutionize the healthcare industry by providing efficient and accurate medical assistance to patients and healthcare professionals. In this literature survey paper, the focus is on the use of Generative Pretrained Transformer (GPT) technology in developing medical consultant chatbots. The objective is to demonstrate the potential for AI-powered chatbots to improve the efficiency and accuracy of medical assistance. The paper provides an overview of the current state of the art medical chatbots, the challenges and limitations of using GPT for medical consultation and insights into future directions for the development of these chatbots are also discussed. Finally, the paper concludes with a discussion of the potential benefits of using GPT-based medical consultant chatbots in providing accessible and efficient health information to patients.},
  keywords={Surveys;Industries;Neural networks;Medical services;Chatbots;Transformers;Reliability;Artificial Intelligence(AI);NLP Transformers;Chatbot;Generative Pretraining Transformer(GPT)},
  doi={10.1109/INCET57972.2023.10170114},
  ISSN={},
  month={May},}@INPROCEEDINGS{8995219,
  author={Pang, Yue and Zhou, Xiangdong and Xu, Donghui and Tan, Zijing and Zhang, Mingxi and Guo, Naiwang and Tian, Yingjie},
  booktitle={2019 IEEE 31st International Conference on Tools with Artificial Intelligence (ICTAI)}, 
  title={Generative Adversarial Learning Based Commercial Building Electricity Time Series Prediction}, 
  year={2019},
  volume={},
  number={},
  pages={1800-1804},
  abstract={Prediction on electricity load time series of commercial building and construction sets plays an important role in smart grid applications, building operation and environmental protection, etc. It differs significantly from the traditional aggregated electricity time series prediction problem and aims to provide more accurate prediction for each building respectively. In recent years, deep learning shows significant improvements on time series prediction over classical methods. However, it suffers from the problem of limited data. The problem is more serious in the context of electricity time series prediction of buildings. For instance, new buildings or buildings with newly installed meters have limited historical data, which results in poor model training and forecasts. In this paper we propose a novel generative adversarial learning based prediction method for dealing with this problem. We employ the generative adversarial network framework to obtain adversarially generated data for prediction model training by using long short-term memory units based recurrent neural network and attention mechanism. Extensive experiments on real-life dataset demonstrate the improvements of our proposed method against the traditional methods.},
  keywords={Training;Deep learning;Recurrent neural networks;Electricity;Buildings;Time series analysis;Predictive models;Data models;Adversarial machine learning;Long short term memory;deep learning;data mining},
  doi={10.1109/ICTAI.2019.00271},
  ISSN={2375-0197},
  month={Nov},}@INPROCEEDINGS{11020513,
  author={Ke, Zong and Zhou, Shicheng and Zhou, Yining and Chang, Chia Hong and Zhang, Rong},
  booktitle={2025 8th International Conference on Advanced Algorithms and Control Engineering (ICAACE)}, 
  title={Detection of AI Deepfake and Fraud in Online Payments Using GAN-Based Models}, 
  year={2025},
  volume={},
  number={},
  pages={1786-1790},
  abstract={This study explores the use of Generative Adversarial Networks (GANs) to detect AI deepfakes and fraudulent activities in online payment systems. With the growing prevalence of deepfake technology, which can manipulate facial features in images and videos, the potential for fraud in online transactions has escalated. Traditional security systems struggle to identify these sophisticated forms of fraud. This research proposes a novel GAN-based model that enhances online payment security by identifying subtle manipulations in payment images. The model is trained on a dataset consisting of real-world online payment images and deepfake images generated using advanced GAN architectures, such as StyleGAN and DeepFake. The results demonstrate that the proposed model can accurately distinguish between legitimate transactions and deepfakes, achieving a high detection rate above 95%. This approach significantly improves the robustness of payment systems against AI-driven fraud. The paper contributes to the growing field of digital security, offering insights into the application of GANs for fraud detection in financial services.},
  keywords={Deepfakes;Adaptation models;Biological system modeling;Speech recognition;Generative adversarial networks;Robustness;Real-time systems;Fraud;Security;Artificial intelligence;Payment Security;Image Recognition;Generative Adversarial Networks;AI Deepfake;Fraudulent Activities},
