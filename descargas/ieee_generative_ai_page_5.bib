  month={June},}@ARTICLE{10634759,
  author={Panyam, Sriram and Gujar, Praveen and Paliwal, Gunjan},
  journal={IEEE Engineering Management Review}, 
  title={Evolving Product Engineering Teams in the AI Era}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={The Artificial Intelligence (AI) revolution, particularly the rise of Generative AI, necessitates a paradigm shift in engineering organizations. This article draws parallels with the cloud revolution, highlighting the need for deeper integrations of AI expertise within engineering teams. We discuss the evolution of product engineering during the cloud era, emphasizing the transition from specialized functional teams to a more integrated model leveraging cloud-native capabilities. We then delve into the challenges and opportunities presented by AI-integrated products, advocating for a distributed model of AI expertise where AI skills are embedded within existing teams. We outline the benefits of this approach, such as accelerated development, deeper AI understanding, and a culture of continuous learning. Finally, we address the challenges of talent gaps, cultural resistance, and integration complexity, offering strategies for mitigation. By embracing a proactive and strategic approach, engineering organizations can successfully navigate the AI revolution and reap its full benefits.},
  keywords={Artificial intelligence;Organizations;Cloud computing;Training;Social networking (online);Product development;Machine learning;cloud computing;product engineering;product development;Artificial Intelligence (AI);organizational transformation;data engineering;machine learning (ML);market leadership;software engineering;managed services},
