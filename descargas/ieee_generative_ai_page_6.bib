  month={April},}@INPROCEEDINGS{11036035,
  author={Liu, Ziyi and Su, Weihua and Zang, Dongyuan and Dang, Xu and Liu, Huixin},
  booktitle={2025 IEEE 6th International Seminar on Artificial Intelligence, Networking and Information Technology (AINIT)}, 
  title={Multi-Module Fusion Vehicle Trajectory Prediction Based on Generative Adversarial Networks}, 
  year={2025},
  volume={},
  number={},
  pages={1412-1416},
  abstract={Vehicle trajectory prediction, as one of the core technologies in intelligent transportation and autonomous driving, plays a significant role in reducing collision risks. However, complex spatiotemporal dependencies and dynamic interactions remain challenging technical issues that require urgent resolution. To address these challenges, this paper proposes a GAN-based multi-module fusion prediction model. By integrating multiple architectures-including a graph attention network, a Transformer encoder, and a temporal convolutional network-the model simultaneously optimizes spatial interactions, temporal dependencies, and trajectory feature modeling. Experimental results demonstrate that, compared with baseline methods such as CV, CS-LSTM, and TCN-LSTM, the proposed model significantly improves prediction accuracy by reducing the average displacement error and final displacement error by 24.38% and 14.46%, respectively, thereby fully validating its effectiveness in complex traffic scenarios.},
  keywords={Deep learning;Visualization;Accuracy;Predictive models;Transformers;Generative adversarial networks;Trajectory;Spatiotemporal phenomena;Vehicle dynamics;Autonomous vehicles;Generative Adversarial Networks;Trajectory Prediction;Deep Learning},
  doi={10.1109/AINIT65432.2025.11036035},
  ISSN={},
  month={April},}@INPROCEEDINGS{9660491,
  author={Li, Jianxue and Xiao, Yang and Wu, Jiawei and Feng, Jialong and Liu, Jun},
  booktitle={2021 7th IEEE International Conference on Network Intelligence and Digital Content (IC-NIDC)}, 
  title={Network Flow Generation Based on Reinforcement Learning Powered Generative Adversarial Network}, 
  year={2021},
  volume={},
  number={},
  pages={235-239},
  abstract={Mining anomalies and special events from massive network flows based on machine learning and deep learning is a promising approach for network management. However, it is difficult to build labeled network flow data sets for training machine learning and deep learning models. In this paper, we propose a novel reinforcement learning (RL) powered generative adversarial network (GAN) model named NF-GAN for network flow generation. The generator of NF-GAN is designed as a stochastic policy model to generate labeled network flow data. In terms of the discriminator, a check reward is integrated into the network reward to capture the correlations among attributes. Experiment results demonstrate that the majority of the generated flows conform to the strict network protocols of the standard OSI stack, and the success rate of network flows generation achieves 99.96%. To the best of our knowledge, this is the first time of applying RL powered GAN on network flow generation tasks.},
  keywords={Knowledge engineering;Deep learning;Training;Protocols;Stochastic processes;Reinforcement learning;Generative adversarial networks;Network flow generation;Generative adversarial network;Reinforcement learning;Dataset augmentation},
  doi={10.1109/IC-NIDC54101.2021.9660491},
  ISSN={2575-4955},
  month={Nov},}@INPROCEEDINGS{11005243,
  author={Choudhary, Vishal Kumar and Pawar, Adesh and Suman, Saurabh and Kumar, Gulshan and Kumar, Abhishek and Kumar, Praveen},
