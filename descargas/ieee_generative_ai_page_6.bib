  title={A Multimodal Emotion Recognition Framework Based on Generative Adversarial Networks}, 
  year={2025},
  volume={},
  number={},
  pages={1670-1673},
  abstract={In recent years, with the rapid development of social media, a large number of open-source emotion recognition datasets have emerged online, providing abundant resources for emotion recognition. Meanwhile, with the help of deep learning, multimodal emotion recognition technology has advanced rapidly. However, deep learning-based multimodal emotion recognition still has some drawbacks, such as requiring a large amount of training data, inconsistencies in the quantity and quality of data within datasets, and heterogeneity among different modalities in multimodal fusion. To address these issues, we propose a multimodal emotion recognition framework based on Generative Adversarial Networks (GANs), which enhances feature representation and reduces heterogeneity in multimodal fusion. Finally, we achieve promising experimental results on the RAVDESS dataset.},
  keywords={Deep learning;Seminars;Emotion recognition;Social networking (online);Training data;Generative adversarial networks;Information technology;Artificial intelligence;Multimodal;Emotion Recognition;deep learning;Generative Adversarial Networks (GANs)},
  doi={10.1109/AINIT65432.2025.11035365},
  ISSN={},
  month={April},}@INPROCEEDINGS{10959222,
  author={Muratovic, Ajdin and Tanovic, Anel and Genjga, Edina},
  booktitle={2025 24th International Symposium INFOTEH-JAHORINA (INFOTEH)}, 
  title={Development of a Methodology for Automated Backend Code Optimization Triggered from a Frontend Web Application Using Generative Artificial Intelligence Models}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={In this paper, the authors introduce an innovative methodology for automating backend code optimization, triggered from the frontend of a web application and powered by advanced generative AI models. The proposed system integrates the Vue.js and Laravel frameworks, together with a PHP (Hypertext preprocessor) Parser and the OpenAI API (Application programming interface), enabling dynamic, real-time optimization without necessitating system downtime. In contrast to conventional approaches that rely on offline environments or CI/CD (Continuous integration and continuous deployment) pipelines, this methodology facilitates continuous performance enhancement during active usage of web application. An experimental evaluation involving 20 functions within a Laravel application demonstrated a 70% optimization success rate, a 62.6% reduction in execution time and an 18.18% decrease in code lines. This approach preserves semantic consistency and provides reports to developers for further implementation. By reducing errors and enabling ongoing refinements, the work makes a significant contribution to the field of web application development.},
  keywords={Codes;Generative AI;Semantics;Pipelines;Manuals;User experience;Real-time systems;Reliability;Optimization;Application programming interfaces;Automation;code optimization;Laravel;Vue.js;artificial intelligence},
  doi={10.1109/INFOTEH64129.2025.10959222},
  ISSN={2767-9470},
  month={March},}@INPROCEEDINGS{10865026,
  author={Zhang, Zhicheng and Wu, Yang and Zheng, Xiaolong},
  booktitle={2024 China Automation Congress (CAC)}, 
  title={Social Intelligence Enhancement Methods for LLM Agents Based on Retrieval-Augmented Generation}, 
  year={2024},
  volume={},
  number={},
  pages={5769-5774},
  abstract={The rapid advancement of Large Language Models (LLMs) has enabled LLM agents to accomplish more complex tasks through interactions. However, in complex social interaction scenarios, LLM agents may still lack a certain degree of social intelligence. This paper proposes the automatic generation of an external repository of social skills by powerful LLMs like GPT-4 to guide agents in social interactions. Based on the Retrieval Augmented Generation (RAG) approach, the proposed method utilizing relevance-based extraction and random sampling to choose appropriate strategies from the social skills repository to enhance the content generation of LLMs. The results demonstrate that this approach can effectively improves the social intelligence of agents across a variety of social interaction scenarios.},
  keywords={Automation;Large language models;Retrieval augmented generation;Social intelligence;large language models;generative agents;social intelligence;retrieval-augmented generation},
  doi={10.1109/CAC63892.2024.10865026},
  ISSN={2688-0938},
  month={Nov},}@INPROCEEDINGS{10932599,
  author={Wadhwa, Shilpa and Sharma, Akhilesh and Ahalawat, Kapil},
  booktitle={2025 2nd International Conference on Computational Intelligence, Communication Technology and Networking (CICTN)}, 
  title={AI-Driven Evolution in HR: A Bibliometric Study on Professional Roles and Skill Adaptation}, 
  year={2025},
  volume={},
  number={},
  pages={225-231},
  abstract={This bibliometric analysis examines the impacts of artificial intelligence (AI) and automation on roles and competencies in human resources (HR), identifying key themes and collaborative patterns in the field. A search conducted in Scopus on October 7, 2024, using “artificial AND intelligence AND automation AND human AND resource AND roles OR skills” initially retrieved 218 documents. Non-English and pre-2015 articles were removed, resulting in a final sample of 208 documents. Core trends and connections between prominent authors, institutions, and countries were mapped through co-authorship, keyword co-occurrence, and citation analyses using VOSviewer software. Findings reveal an international focus on skill adaptation, digital transformation, and AI-augmented HR competencies, highlighting an evolving collaborative effort to address automation challenges. This study enhances bibliometric literature, paving the way for future HR research in the AI era.},
  keywords={Hands;Ethics;Automation;Digital transformation;Collaboration;Market research;Software;Hazards;Personnel;Artificial intelligence;Skills development;VOSviewer;Scopus database;human resource competencies;workforce automation;co-authorship analysis;bibliography analysis;artificial intelligence;automation;bibliometric analysis;digital transformation},
  doi={10.1109/CICTN64563.2025.10932599},
  ISSN={},
  month={Feb},}@INBOOK{10948944,
