  month={June},}@INPROCEEDINGS{10987445,
  author={Shetty, Anusha Prashanth and L, Deepthi and Puthran, Raksha and Shetty, Surendra and Parveez, Mohammad Anzil and Kumar, Shravan},
  booktitle={2025 International Conference on Artificial Intelligence and Data Engineering (AIDE)}, 
  title={Comparision of Principal Component Analysis, K-means, and Generative Adversarial Network for Compression and reconstruction of medical image}, 
  year={2025},
  volume={},
  number={},
  pages={79-84},
  abstract={Medical images are essential in healthcare, but their large file sizes present challenges for storage and transmission. This study proposes a framework for compressing color medical images while preserving key visual information. We apply three compression models: Principal Component Analysis (PCA), K-Means clustering, and Generative Adversarial Networks (GANs), and evaluate their performance based on Compression Ratio (CR), Peak Signal-to-Noise Ratio (PSNR), Mean Squared Error (MSE), and Structural Similarity Index (SSIM). K-Means with 100 clusters outperforms the other models, achieving the highest compression (CR of 1.20), with excellent image quality (PSNR of 66.77 dB, SSIM of 1.0000). PCA shows moderate performance with a CR of 2.45, while GAN provides the least effective compression and image quality. These results demonstrate that K-Means offers the best trade-off between compression and image fidelity.},
  keywords={Measurement;Image quality;Analytical models;Image coding;PSNR;Medical services;Generative adversarial networks;Image reconstruction;Principal component analysis;Biomedical imaging;Medical images;PCA;K-Means;GAN},
